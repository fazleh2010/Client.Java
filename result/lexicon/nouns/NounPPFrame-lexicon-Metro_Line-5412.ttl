@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Metro_Line-5412 ;
  lemon:entry    :Metro_Line-5412_form_preposition .

:Metro_Line-5412 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Metro_Line-5412_form ;
  lemon:otherForm  :Metro_Line-5412_singular ;
  lemon:otherForm  :Metro_Line-5412_plural ;
  lemon:sense          :Metro_Line-5412_sense_1 ;
  lemon:synBehavior    :Metro_Line-5412_nounpp .

:Metro_Line-5412_form a lemon:Form ;
  lemon:writtenRep "Metro Line"@en .

:Metro_Line-5412_singular a    lemon:Form ;
  lemon:writtenRep "Metro Line"@en ;
  lexinfo:number   lexinfo:singular .

:Metro_Line-5412_plural a   lemon:Form ;
  lemon:writtenRep "Metro Line"@en ;
  lexinfo:number    lexinfo:plural .

:Metro_Line-5412_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Metro_Line-5412_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Metro_Line-5412_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/openingDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Metro_Line-5412_sense_1_condition .

:Metro_Line-5412_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :Metro_Line-5412_form_preposition .
## Prepositions ##
:Metro_Line-5412_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

