@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :speedway-4283 ;
  lemon:entry    :speedway-4283_form_preposition .

:speedway-4283 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :speedway-4283_form ;
  lemon:otherForm  :speedway-4283_singular ;
  lemon:otherForm  :speedway-4283_plural ;
  lemon:sense          :speedway-4283_sense_1 ;
  lemon:synBehavior    :speedway-4283_nounpp .

:speedway-4283_form a lemon:Form ;
  lemon:writtenRep "speedway"@en .

:speedway-4283_singular a    lemon:Form ;
  lemon:writtenRep "speedway"@en ;
  lexinfo:number   lexinfo:singular .

:speedway-4283_plural a   lemon:Form ;
  lemon:writtenRep "speedway"@en ;
  lexinfo:number    lexinfo:plural .

:speedway-4283_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:speedway-4283_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :speedway-4283_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/league> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
