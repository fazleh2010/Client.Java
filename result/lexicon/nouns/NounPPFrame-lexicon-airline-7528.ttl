@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :airline-7528 ;
  lemon:entry    :airline-7528_form_preposition .

:airline-7528 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :airline-7528_form ;
  lemon:otherForm  :airline-7528_singular ;
  lemon:otherForm  :airline-7528_plural ;
  lemon:sense          :airline-7528_sense_1 ;
  lemon:synBehavior    :airline-7528_nounpp .

:airline-7528_form a lemon:Form ;
  lemon:writtenRep "airline"@en .

:airline-7528_singular a    lemon:Form ;
  lemon:writtenRep "airline"@en ;
  lexinfo:number   lexinfo:singular .

:airline-7528_plural a   lemon:Form ;
  lemon:writtenRep "airline"@en ;
  lexinfo:number    lexinfo:plural .

:airline-7528_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:airline-7528_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :airline-7528_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/targetAirport> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :airline-7528_sense_1_condition .

:airline-7528_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Airline> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Airport> .

:arg2 lemon:marker :airline-7528_form_preposition .
## Prepositions ##
:airline-7528_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

