  lemon:canonicalForm  :'s_county_seat-4125_form ;
  lemon:otherForm  :'s_county_seat-4125_singular ;
  lemon:otherForm  :'s_county_seat-4125_plural ;
  lemon:sense          :'s_county_seat-4125_sense_1 ;
  lemon:synBehavior    :'s_county_seat-4125_nounpp .

