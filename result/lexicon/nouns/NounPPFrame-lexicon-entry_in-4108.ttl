@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :entry_in-4108 ;
  lemon:entry    :entry_in-4108_form_preposition .

:entry_in-4108 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :entry_in-4108_form ;
  lemon:otherForm  :entry_in-4108_singular ;
  lemon:otherForm  :entry_in-4108_plural ;
  lemon:sense          :entry_in-4108_sense_1 ;
  lemon:synBehavior    :entry_in-4108_nounpp .

:entry_in-4108_form a lemon:Form ;
  lemon:writtenRep "entry in"@en .

:entry_in-4108_singular a    lemon:Form ;
  lemon:writtenRep "entry in"@en ;
  lexinfo:number   lexinfo:singular .

:entry_in-4108_plural a   lemon:Form ;
  lemon:writtenRep "entry in"@en ;
  lexinfo:number    lexinfo:plural .

:entry_in-4108_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:entry_in-4108_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :entry_in-4108_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/language> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :entry_in-4108_sense_1_condition .

:entry_in-4108_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Language> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :entry_in-4108_form_preposition .
## Prepositions ##
:entry_in-4108_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

