@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :company-6218 ;
  lemon:entry    :company-6218_form_preposition .

:company-6218 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :company-6218_form ;
  lemon:otherForm  :company-6218_singular ;
  lemon:otherForm  :company-6218_plural ;
  lemon:sense          :company-6218_sense_1 ;
  lemon:synBehavior    :company-6218_nounpp .

:company-6218_form a lemon:Form ;
  lemon:writtenRep "company"@en .

:company-6218_singular a    lemon:Form ;
  lemon:writtenRep "company"@en ;
  lexinfo:number   lexinfo:singular .

:company-6218_plural a   lemon:Form ;
  lemon:writtenRep "company"@en ;
  lexinfo:number    lexinfo:plural .

:company-6218_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:company-6218_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :company-6218_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/product> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :company-6218_sense_1_condition .

:company-6218_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Ship> .

:arg2 lemon:marker :company-6218_form_preposition .
## Prepositions ##
:company-6218_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

