@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :series-5258 ;
  lemon:entry    :series-5258_form_preposition .

:series-5258 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :series-5258_form ;
  lemon:otherForm  :series-5258_singular ;
  lemon:otherForm  :series-5258_plural ;
  lemon:sense          :series-5258_sense_1 ;
  lemon:synBehavior    :series-5258_nounpp .

:series-5258_form a lemon:Form ;
  lemon:writtenRep "series"@en .

:series-5258_singular a    lemon:Form ;
  lemon:writtenRep "series"@en ;
  lexinfo:number   lexinfo:singular .

:series-5258_plural a   lemon:Form ;
  lemon:writtenRep "series"@en ;
  lexinfo:number    lexinfo:plural .

:series-5258_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:series-5258_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :series-5258_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/numberOfSeasons> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :series-5258_sense_1_condition .

:series-5258_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#nonNegativeInteger> .

:arg2 lemon:marker :series-5258_form_preposition .
## Prepositions ##
:series-5258_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

