@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :sea_level-428 ;
  lemon:entry    :sea_level-428_form_preposition .

:sea_level-428 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :sea_level-428_form ;
  lemon:otherForm  :sea_level-428_singular ;
  lemon:otherForm  :sea_level-428_plural ;
  lemon:sense          :sea_level-428_sense_1 ;
  lemon:synBehavior    :sea_level-428_nounpp .

:sea_level-428_form a lemon:Form ;
  lemon:writtenRep "sea level"@en .

:sea_level-428_singular a    lemon:Form ;
  lemon:writtenRep "sea level"@en ;
  lexinfo:number   lexinfo:singular .

:sea_level-428_plural a   lemon:Form ;
  lemon:writtenRep "sea level"@en ;
  lexinfo:number    lexinfo:plural .

:sea_level-428_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:sea_level-428_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :sea_level-428_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/areaCode> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :sea_level-428_sense_1_condition .

:sea_level-428_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :sea_level-428_form_preposition .
## Prepositions ##
:sea_level-428_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

