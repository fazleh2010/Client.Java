@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :grape-8109 ;
  lemon:entry    :grape-8109_form_preposition .

:grape-8109 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :grape-8109_form ;
  lemon:otherForm  :grape-8109_singular ;
  lemon:otherForm  :grape-8109_plural ;
  lemon:sense          :grape-8109_sense_1 ;
  lemon:synBehavior    :grape-8109_nounpp .

:grape-8109_form a lemon:Form ;
  lemon:writtenRep "grape"@en .

:grape-8109_singular a    lemon:Form ;
  lemon:writtenRep "grape"@en ;
  lexinfo:number   lexinfo:singular .

:grape-8109_plural a   lemon:Form ;
  lemon:writtenRep "grape"@en ;
  lexinfo:number    lexinfo:plural .

:grape-8109_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:grape-8109_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :grape-8109_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/wineRegion> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :grape-8109_sense_1_condition .

:grape-8109_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Grape> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Place> .

:arg2 lemon:marker :grape-8109_form_preposition .
## Prepositions ##
:grape-8109_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

