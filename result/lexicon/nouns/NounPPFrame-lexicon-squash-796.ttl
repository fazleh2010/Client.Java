@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :squash-796 ;
  lemon:entry    :squash-796_form_preposition .

:squash-796 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :squash-796_form ;
  lemon:otherForm  :squash-796_singular ;
  lemon:otherForm  :squash-796_plural ;
  lemon:sense          :squash-796_sense_1 ;
  lemon:synBehavior    :squash-796_nounpp .

:squash-796_form a lemon:Form ;
  lemon:writtenRep "squash"@en .

:squash-796_singular a    lemon:Form ;
  lemon:writtenRep "squash"@en ;
  lexinfo:number   lexinfo:singular .

:squash-796_plural a   lemon:Form ;
  lemon:writtenRep "squash"@en ;
  lexinfo:number    lexinfo:plural .

:squash-796_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:squash-796_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :squash-796_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/birthPlace> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :squash-796_sense_1_condition .

:squash-796_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/resource/Surfing> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :squash-796_form_preposition .
## Prepositions ##
:squash-796_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

