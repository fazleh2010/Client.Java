@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :St_Mark-6592 ;
  lemon:entry    :St_Mark-6592_form_preposition .

:St_Mark-6592 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :St_Mark-6592_form ;
  lemon:otherForm  :St_Mark-6592_singular ;
  lemon:otherForm  :St_Mark-6592_plural ;
  lemon:sense          :St_Mark-6592_sense_1 ;
  lemon:synBehavior    :St_Mark-6592_nounpp .

:St_Mark-6592_form a lemon:Form ;
  lemon:writtenRep "St Mark"@en .

:St_Mark-6592_singular a    lemon:Form ;
  lemon:writtenRep "St Mark"@en ;
  lexinfo:number   lexinfo:singular .

:St_Mark-6592_plural a   lemon:Form ;
  lemon:writtenRep "St Mark"@en ;
  lexinfo:number    lexinfo:plural .

:St_Mark-6592_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:St_Mark-6592_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :St_Mark-6592_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/residence> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :St_Mark-6592_sense_1_condition .

:St_Mark-6592_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Single> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :St_Mark-6592_form_preposition .
## Prepositions ##
:St_Mark-6592_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

