@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :figure-21099 ;
  lemon:entry    :figure-21099_form_preposition .

:figure-21099 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :figure-21099_form ;
  lemon:otherForm  :figure-21099_singular ;
  lemon:otherForm  :figure-21099_plural ;
  lemon:sense          :figure-21099_sense_1 ;
  lemon:synBehavior    :figure-21099_nounpp .

:figure-21099_form a lemon:Form ;
  lemon:writtenRep "figure"@en .

:figure-21099_singular a    lemon:Form ;
  lemon:writtenRep "figure"@en ;
  lexinfo:number   lexinfo:singular .

:figure-21099_plural a   lemon:Form ;
  lemon:writtenRep "figure"@en ;
  lexinfo:number    lexinfo:plural .

:figure-21099_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:figure-21099_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :figure-21099_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/country> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :figure-21099_sense_1_condition .

:figure-21099_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :figure-21099_form_preposition .
## Prepositions ##
:figure-21099_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

