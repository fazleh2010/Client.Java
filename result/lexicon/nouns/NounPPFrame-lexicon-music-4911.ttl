@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :music-4911 ;
  lemon:entry    :music-4911_form_preposition .

:music-4911 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :music-4911_form ;
  lemon:otherForm  :music-4911_singular ;
  lemon:otherForm  :music-4911_plural ;
  lemon:sense          :music-4911_sense_1 ;
  lemon:synBehavior    :music-4911_nounpp .

:music-4911_form a lemon:Form ;
  lemon:writtenRep "music"@en .

:music-4911_singular a    lemon:Form ;
  lemon:writtenRep "music"@en ;
  lexinfo:number   lexinfo:singular .

:music-4911_plural a   lemon:Form ;
  lemon:writtenRep "music"@en ;
  lexinfo:number    lexinfo:plural .

:music-4911_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:music-4911_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :music-4911_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/musicBy> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :music-4911_sense_1_condition .

:music-4911_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Musical> .

:arg2 lemon:marker :music-4911_form_preposition .
## Prepositions ##
:music-4911_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

