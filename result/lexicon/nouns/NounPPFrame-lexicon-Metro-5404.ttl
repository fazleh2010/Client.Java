@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Metro-5404 ;
  lemon:entry    :Metro-5404_form_preposition .

:Metro-5404 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Metro-5404_form ;
  lemon:otherForm  :Metro-5404_singular ;
  lemon:otherForm  :Metro-5404_plural ;
  lemon:sense          :Metro-5404_sense_1 ;
  lemon:synBehavior    :Metro-5404_nounpp .

:Metro-5404_form a lemon:Form ;
  lemon:writtenRep "Metro"@en .

:Metro-5404_singular a    lemon:Form ;
  lemon:writtenRep "Metro"@en ;
  lexinfo:number   lexinfo:singular .

:Metro-5404_plural a   lemon:Form ;
  lemon:writtenRep "Metro"@en ;
  lexinfo:number    lexinfo:plural .

:Metro-5404_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Metro-5404_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Metro-5404_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/openingDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Metro-5404_sense_1_condition .

:Metro-5404_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :Metro-5404_form_preposition .
## Prepositions ##
:Metro-5404_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

