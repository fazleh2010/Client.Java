@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :District-2155 ;
  lemon:entry    :District-2155_form_preposition .

:District-2155 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :District-2155_form ;
  lemon:otherForm  :District-2155_singular ;
  lemon:otherForm  :District-2155_plural ;
  lemon:sense          :District-2155_sense_1 ;
  lemon:synBehavior    :District-2155_nounpp .

:District-2155_form a lemon:Form ;
  lemon:writtenRep "District"@en .

:District-2155_singular a    lemon:Form ;
  lemon:writtenRep "District"@en ;
  lexinfo:number   lexinfo:singular .

:District-2155_plural a   lemon:Form ;
  lemon:writtenRep "District"@en ;
  lexinfo:number    lexinfo:plural .

:District-2155_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:District-2155_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :District-2155_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/dissolutionDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :District-2155_sense_1_condition .

:District-2155_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :District-2155_form_preposition .
## Prepositions ##
:District-2155_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

