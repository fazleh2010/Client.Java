@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Song-4099 ;
  lemon:entry    :Song-4099_form_preposition .

:Song-4099 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Song-4099_form ;
  lemon:otherForm  :Song-4099_singular ;
  lemon:otherForm  :Song-4099_plural ;
  lemon:sense          :Song-4099_sense_1 ;
  lemon:synBehavior    :Song-4099_nounpp .

:Song-4099_form a lemon:Form ;
  lemon:writtenRep "Song"@en .

:Song-4099_singular a    lemon:Form ;
  lemon:writtenRep "Song"@en ;
  lexinfo:number   lexinfo:singular .

:Song-4099_plural a   lemon:Form ;
  lemon:writtenRep "Song"@en ;
  lexinfo:number    lexinfo:plural .

:Song-4099_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Song-4099_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Song-4099_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/language> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Song-4099_sense_1_condition .

:Song-4099_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Language> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :Song-4099_form_preposition .
## Prepositions ##
:Song-4099_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

