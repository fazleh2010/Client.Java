@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Premier_of-3218 ;
  lemon:entry    :Premier_of-3218_form_preposition .

:Premier_of-3218 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Premier_of-3218_form ;
  lemon:otherForm  :Premier_of-3218_singular ;
  lemon:otherForm  :Premier_of-3218_plural ;
  lemon:sense          :Premier_of-3218_sense_1 ;
  lemon:synBehavior    :Premier_of-3218_nounpp .

:Premier_of-3218_form a lemon:Form ;
  lemon:writtenRep "Premier of"@en .

:Premier_of-3218_singular a    lemon:Form ;
  lemon:writtenRep "Premier of"@en ;
  lexinfo:number   lexinfo:singular .

:Premier_of-3218_plural a   lemon:Form ;
  lemon:writtenRep "Premier of"@en ;
  lexinfo:number    lexinfo:plural .

:Premier_of-3218_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Premier_of-3218_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Premier_of-3218_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/governor> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Premier_of-3218_sense_1_condition .

:Premier_of-3218_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :Premier_of-3218_form_preposition .
## Prepositions ##
:Premier_of-3218_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

