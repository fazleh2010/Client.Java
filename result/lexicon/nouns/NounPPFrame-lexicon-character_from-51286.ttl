@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :character_from-51286 ;
  lemon:entry    :character_from-51286_form_preposition .

:character_from-51286 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :character_from-51286_form ;
  lemon:otherForm  :character_from-51286_singular ;
  lemon:otherForm  :character_from-51286_plural ;
  lemon:sense          :character_from-51286_sense_1 ;
  lemon:synBehavior    :character_from-51286_nounpp .

:character_from-51286_form a lemon:Form ;
  lemon:writtenRep "character from"@en .

:character_from-51286_singular a    lemon:Form ;
  lemon:writtenRep "character from"@en ;
  lexinfo:number   lexinfo:singular .

:character_from-51286_plural a   lemon:Form ;
  lemon:writtenRep "character from"@en ;
  lexinfo:number    lexinfo:plural .

:character_from-51286_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:character_from-51286_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :character_from-51286_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/portrayer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :character_from-51286_sense_1_condition .

:character_from-51286_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/FictionalCharacter> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :character_from-51286_form_preposition .
## Prepositions ##
:character_from-51286_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

