@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Mark-4992 ;
  lemon:entry    :Mark-4992_form_preposition .

:Mark-4992 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Mark-4992_form ;
  lemon:otherForm  :Mark-4992_singular ;
  lemon:otherForm  :Mark-4992_plural ;
  lemon:sense          :Mark-4992_sense_1 ;
  lemon:synBehavior    :Mark-4992_nounpp .

:Mark-4992_form a lemon:Form ;
  lemon:writtenRep "Mark"@en .

:Mark-4992_singular a    lemon:Form ;
  lemon:writtenRep "Mark"@en ;
  lexinfo:number   lexinfo:singular .

:Mark-4992_plural a   lemon:Form ;
  lemon:writtenRep "Mark"@en ;
  lexinfo:number    lexinfo:plural .

:Mark-4992_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Mark-4992_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Mark-4992_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/nationality> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Mark-4992_sense_1_condition .

:Mark-4992_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :Mark-4992_form_preposition .
## Prepositions ##
:Mark-4992_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

