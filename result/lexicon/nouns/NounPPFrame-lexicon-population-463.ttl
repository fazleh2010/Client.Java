@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :population-463 ;
  lemon:entry    :population-463_form_preposition .

:population-463 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :population-463_form ;
  lemon:otherForm  :population-463_singular ;
  lemon:otherForm  :population-463_plural ;
  lemon:sense          :population-463_sense_1 ;
  lemon:synBehavior    :population-463_nounpp .

:population-463_form a lemon:Form ;
  lemon:writtenRep "population"@en .

:population-463_singular a    lemon:Form ;
  lemon:writtenRep "population"@en ;
  lexinfo:number   lexinfo:singular .

:population-463_plural a   lemon:Form ;
  lemon:writtenRep "population"@en ;
  lexinfo:number    lexinfo:plural .

:population-463_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:population-463_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :population-463_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/areaTotal> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :population-463_sense_1_condition .

:population-463_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :population-463_form_preposition .
## Prepositions ##
:population-463_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

