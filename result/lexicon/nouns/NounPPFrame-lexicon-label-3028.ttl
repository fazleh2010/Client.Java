@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :label-3028 ;
  lemon:entry    :label-3028_form_preposition .

:label-3028 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :label-3028_form ;
  lemon:otherForm  :label-3028_singular ;
  lemon:otherForm  :label-3028_plural ;
  lemon:sense          :label-3028_sense_1 ;
  lemon:synBehavior    :label-3028_nounpp .

:label-3028_form a lemon:Form ;
  lemon:writtenRep "label"@en .

:label-3028_singular a    lemon:Form ;
  lemon:writtenRep "label"@en ;
  lexinfo:number   lexinfo:singular .

:label-3028_plural a   lemon:Form ;
  lemon:writtenRep "label"@en ;
  lexinfo:number    lexinfo:plural .

:label-3028_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:label-3028_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :label-3028_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/foundedBy> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :label-3028_sense_1_condition .

:label-3028_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Agent> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Pope> .

:arg2 lemon:marker :label-3028_form_preposition .
## Prepositions ##
:label-3028_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

