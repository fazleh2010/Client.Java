@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :designated_Site_of-5726 ;
  lemon:entry    :designated_Site_of-5726_form_preposition .

:designated_Site_of-5726 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :designated_Site_of-5726_form ;
  lemon:otherForm  :designated_Site_of-5726_singular ;
  lemon:otherForm  :designated_Site_of-5726_plural ;
  lemon:sense          :designated_Site_of-5726_sense_1 ;
  lemon:synBehavior    :designated_Site_of-5726_nounpp .

:designated_Site_of-5726_form a lemon:Form ;
  lemon:writtenRep "designated Site of"@en .

:designated_Site_of-5726_singular a    lemon:Form ;
  lemon:writtenRep "designated Site of"@en ;
  lexinfo:number   lexinfo:singular .

:designated_Site_of-5726_plural a   lemon:Form ;
  lemon:writtenRep "designated Site of"@en ;
  lexinfo:number    lexinfo:plural .

:designated_Site_of-5726_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:designated_Site_of-5726_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :designated_Site_of-5726_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/minimumTemperature> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :designated_Site_of-5726_sense_1_condition .

:designated_Site_of-5726_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Planet> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :designated_Site_of-5726_form_preposition .
## Prepositions ##
:designated_Site_of-5726_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

