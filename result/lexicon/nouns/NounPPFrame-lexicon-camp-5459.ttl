@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :camp-5459 ;
  lemon:entry    :camp-5459_form_preposition .

:camp-5459 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :camp-5459_form ;
  lemon:otherForm  :camp-5459_singular ;
  lemon:otherForm  :camp-5459_plural ;
  lemon:sense          :camp-5459_sense_1 ;
  lemon:synBehavior    :camp-5459_nounpp .

:camp-5459_form a lemon:Form ;
  lemon:writtenRep "camp"@en .

:camp-5459_singular a    lemon:Form ;
  lemon:writtenRep "camp"@en ;
  lexinfo:number   lexinfo:singular .

:camp-5459_plural a   lemon:Form ;
  lemon:writtenRep "camp"@en ;
  lexinfo:number    lexinfo:plural .

:camp-5459_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:camp-5459_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :camp-5459_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/operator> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :camp-5459_sense_1_condition .

:camp-5459_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/LaunchPad> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Organisation> .

:arg2 lemon:marker :camp-5459_form_preposition .
## Prepositions ##
:camp-5459_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

