@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :pianist-1905 ;
  lemon:entry    :pianist-1905_form_preposition .

:pianist-1905 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :pianist-1905_form ;
  lemon:otherForm  :pianist-1905_singular ;
  lemon:otherForm  :pianist-1905_plural ;
  lemon:sense          :pianist-1905_sense_1 ;
  lemon:synBehavior    :pianist-1905_nounpp .

:pianist-1905_form a lemon:Form ;
  lemon:writtenRep "pianist"@en .

:pianist-1905_singular a    lemon:Form ;
  lemon:writtenRep "pianist"@en ;
  lexinfo:number   lexinfo:singular .

:pianist-1905_plural a   lemon:Form ;
  lemon:writtenRep "pianist"@en ;
  lexinfo:number    lexinfo:plural .

:pianist-1905_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:pianist-1905_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :pianist-1905_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/deathPlace> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :pianist-1905_sense_1_condition .

:pianist-1905_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Politician> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :pianist-1905_form_preposition .
## Prepositions ##
:pianist-1905_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

