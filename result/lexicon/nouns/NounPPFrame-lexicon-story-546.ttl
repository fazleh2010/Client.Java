@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :story-546 ;
  lemon:entry    :story-546_form_preposition .

:story-546 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :story-546_form ;
  lemon:otherForm  :story-546_singular ;
  lemon:otherForm  :story-546_plural ;
  lemon:sense          :story-546_sense_1 ;
  lemon:synBehavior    :story-546_nounpp .

:story-546_form a lemon:Form ;
  lemon:writtenRep "story"@en .

:story-546_singular a    lemon:Form ;
  lemon:writtenRep "story"@en ;
  lexinfo:number   lexinfo:singular .

:story-546_plural a   lemon:Form ;
  lemon:writtenRep "story"@en ;
  lexinfo:number    lexinfo:plural .

:story-546_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:story-546_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :story-546_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/author> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :story-546_sense_1_condition .

:story-546_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Book> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :story-546_form_preposition .
## Prepositions ##
:story-546_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

