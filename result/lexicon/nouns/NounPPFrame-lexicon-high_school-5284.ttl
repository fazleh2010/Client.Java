@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :high_school-5284 ;
  lemon:entry    :high_school-5284_form_preposition .

:high_school-5284 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :high_school-5284_form ;
  lemon:otherForm  :high_school-5284_singular ;
  lemon:otherForm  :high_school-5284_plural ;
  lemon:sense          :high_school-5284_sense_1 ;
  lemon:synBehavior    :high_school-5284_nounpp .

:high_school-5284_form a lemon:Form ;
  lemon:writtenRep "high school"@en .

:high_school-5284_singular a    lemon:Form ;
