  lexinfo:number   lexinfo:singular .

:high_school-5284_plural a   lemon:Form ;
  lemon:writtenRep "high school"@en ;
  lexinfo:number    lexinfo:plural .

:high_school-5284_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:high_school-5284_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
