@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Airport-2530 ;
  lemon:entry    :Airport-2530_form_preposition .

:Airport-2530 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Airport-2530_form ;
  lemon:otherForm  :Airport-2530_singular ;
  lemon:otherForm  :Airport-2530_plural ;
  lemon:sense          :Airport-2530_sense_1 ;
  lemon:synBehavior    :Airport-2530_nounpp .

:Airport-2530_form a lemon:Form ;
  lemon:writtenRep "Airport"@en .

:Airport-2530_singular a    lemon:Form ;
  lemon:writtenRep "Airport"@en ;
  lexinfo:number   lexinfo:singular .

:Airport-2530_plural a   lemon:Form ;
  lemon:writtenRep "Airport"@en ;
  lexinfo:number    lexinfo:plural .

:Airport-2530_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Airport-2530_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Airport-2530_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/elevation> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Airport-2530_sense_1_condition .

:Airport-2530_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :Airport-2530_form_preposition .
## Prepositions ##
:Airport-2530_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

