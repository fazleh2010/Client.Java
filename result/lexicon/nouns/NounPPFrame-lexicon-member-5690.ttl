@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :member-5690 ;
  lemon:entry    :member-5690_form_preposition .

:member-5690 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :member-5690_form ;
  lemon:otherForm  :member-5690_singular ;
  lemon:otherForm  :member-5690_plural ;
  lemon:sense          :member-5690_sense_1 ;
  lemon:synBehavior    :member-5690_nounpp .

:member-5690_form a lemon:Form ;
  lemon:writtenRep "member"@en .

:member-5690_singular a    lemon:Form ;
  lemon:writtenRep "member"@en ;
  lexinfo:number   lexinfo:singular .

:member-5690_plural a   lemon:Form ;
  lemon:writtenRep "member"@en ;
  lexinfo:number    lexinfo:plural .

:member-5690_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:member-5690_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :member-5690_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/party> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :member-5690_sense_1_condition .

:member-5690_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/PoliticalParty> .

:arg2 lemon:marker :member-5690_form_preposition .
## Prepositions ##
:member-5690_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

