@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :seat-4122 ;
  lemon:entry    :seat-4122_form_preposition .

:seat-4122 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :seat-4122_form ;
  lemon:otherForm  :seat-4122_singular ;
  lemon:otherForm  :seat-4122_plural ;
  lemon:sense          :seat-4122_sense_1 ;
  lemon:synBehavior    :seat-4122_nounpp .

:seat-4122_form a lemon:Form ;
  lemon:writtenRep "seat"@en .

:seat-4122_singular a    lemon:Form ;
  lemon:writtenRep "seat"@en ;
  lexinfo:number   lexinfo:singular .

:seat-4122_plural a   lemon:Form ;
  lemon:writtenRep "seat"@en ;
  lexinfo:number    lexinfo:plural .

:seat-4122_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:seat-4122_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :seat-4122_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/largestCity> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :seat-4122_sense_1_condition .

:seat-4122_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :seat-4122_form_preposition .
## Prepositions ##
:seat-4122_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

