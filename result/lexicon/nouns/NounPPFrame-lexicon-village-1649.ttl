@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :village-1649 ;
  lemon:entry    :village-1649_form_preposition .

:village-1649 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :village-1649_form ;
  lemon:otherForm  :village-1649_singular ;
  lemon:otherForm  :village-1649_plural ;
  lemon:sense          :village-1649_sense_1 ;
  lemon:synBehavior    :village-1649_nounpp .

:village-1649_form a lemon:Form ;
  lemon:writtenRep "village"@en .

:village-1649_singular a    lemon:Form ;
  lemon:writtenRep "village"@en ;
  lexinfo:number   lexinfo:singular .

:village-1649_plural a   lemon:Form ;
  lemon:writtenRep "village"@en ;
  lexinfo:number    lexinfo:plural .

:village-1649_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:village-1649_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :village-1649_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/country> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :village-1649_sense_1_condition .

:village-1649_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :village-1649_form_preposition .
## Prepositions ##
:village-1649_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
