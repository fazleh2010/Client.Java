@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :sq_mi-2606 ;
  lemon:entry    :sq_mi-2606_form_preposition .

:sq_mi-2606 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :sq_mi-2606_form ;
  lemon:otherForm  :sq_mi-2606_singular ;
  lemon:otherForm  :sq_mi-2606_plural ;
  lemon:sense          :sq_mi-2606_sense_1 ;
  lemon:synBehavior    :sq_mi-2606_nounpp .

:sq_mi-2606_form a lemon:Form ;
  lemon:writtenRep "sq mi"@en .

:sq_mi-2606_singular a    lemon:Form ;
  lemon:writtenRep "sq mi"@en ;
  lexinfo:number   lexinfo:singular .

:sq_mi-2606_plural a   lemon:Form ;
  lemon:writtenRep "sq mi"@en ;
  lexinfo:number    lexinfo:plural .

:sq_mi-2606_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:sq_mi-2606_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :sq_mi-2606_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/ethnicGroup> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :sq_mi-2606_sense_1_condition .

:sq_mi-2606_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/EthnicGroup> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :sq_mi-2606_form_preposition .
## Prepositions ##
:sq_mi-2606_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

