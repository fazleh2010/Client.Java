@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :record-3029 ;
  lemon:entry    :record-3029_form_preposition .

:record-3029 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :record-3029_form ;
  lemon:otherForm  :record-3029_singular ;
  lemon:otherForm  :record-3029_plural ;
  lemon:sense          :record-3029_sense_1 ;
  lemon:synBehavior    :record-3029_nounpp .

:record-3029_form a lemon:Form ;
  lemon:writtenRep "record"@en .

:record-3029_singular a    lemon:Form ;
  lemon:writtenRep "record"@en ;
  lexinfo:number   lexinfo:singular .

:record-3029_plural a   lemon:Form ;
  lemon:writtenRep "record"@en ;
  lexinfo:number    lexinfo:plural .

:record-3029_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:record-3029_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :record-3029_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/foundedBy> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :record-3029_sense_1_condition .

:record-3029_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Agent> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Pope> .

:arg2 lemon:marker :record-3029_form_preposition .
## Prepositions ##
:record-3029_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

