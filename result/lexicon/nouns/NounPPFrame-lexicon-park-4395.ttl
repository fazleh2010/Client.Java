@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :park-4395 ;
  lemon:entry    :park-4395_form_preposition .

:park-4395 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :park-4395_form ;
  lemon:otherForm  :park-4395_singular ;
  lemon:otherForm  :park-4395_plural ;
  lemon:sense          :park-4395_sense_1 ;
  lemon:synBehavior    :park-4395_nounpp .

:park-4395_form a lemon:Form ;
  lemon:writtenRep "park"@en .

:park-4395_singular a    lemon:Form ;
  lemon:writtenRep "park"@en ;
  lexinfo:number   lexinfo:singular .

:park-4395_plural a   lemon:Form ;
  lemon:writtenRep "park"@en ;
  lexinfo:number    lexinfo:plural .

:park-4395_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:park-4395_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :park-4395_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/locatedInArea> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :park-4395_sense_1_condition .

:park-4395_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :park-4395_form_preposition .
## Prepositions ##
:park-4395_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

