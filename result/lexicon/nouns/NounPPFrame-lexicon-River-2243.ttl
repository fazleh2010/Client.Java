@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :River-2243 ;
  lemon:entry    :River-2243_form_preposition .

:River-2243 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :River-2243_form ;
  lemon:otherForm  :River-2243_singular ;
  lemon:otherForm  :River-2243_plural ;
  lemon:sense          :River-2243_sense_1 ;
  lemon:synBehavior    :River-2243_nounpp .

:River-2243_form a lemon:Form ;
  lemon:writtenRep "River"@en .

:River-2243_singular a    lemon:Form ;
  lemon:writtenRep "River"@en ;
  lexinfo:number   lexinfo:singular .

:River-2243_plural a   lemon:Form ;
  lemon:writtenRep "River"@en ;
  lexinfo:number    lexinfo:plural .

:River-2243_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:River-2243_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :River-2243_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/crosses> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :River-2243_sense_1_condition .

:River-2243_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Bridge> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/River> .

:arg2 lemon:marker :River-2243_form_preposition .
## Prepositions ##
:River-2243_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

