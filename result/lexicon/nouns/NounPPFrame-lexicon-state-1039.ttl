@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :state-1039 ;
  lemon:entry    :state-1039_form_preposition .

:state-1039 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :state-1039_form ;
  lemon:otherForm  :state-1039_singular ;
  lemon:otherForm  :state-1039_plural ;
  lemon:sense          :state-1039_sense_1 ;
  lemon:synBehavior    :state-1039_nounpp .

:state-1039_form a lemon:Form ;
  lemon:writtenRep "state"@en .

:state-1039_singular a    lemon:Form ;
  lemon:writtenRep "state"@en ;
  lexinfo:number   lexinfo:singular .

:state-1039_plural a   lemon:Form ;
  lemon:writtenRep "state"@en ;
  lexinfo:number    lexinfo:plural .

:state-1039_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:state-1039_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :state-1039_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/capital> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :state-1039_sense_1_condition .

:state-1039_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :state-1039_form_preposition .
## Prepositions ##
:state-1039_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

