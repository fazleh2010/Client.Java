@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :party-5695 ;
  lemon:entry    :party-5695_form_preposition .

:party-5695 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :party-5695_form ;
  lemon:otherForm  :party-5695_singular ;
  lemon:otherForm  :party-5695_plural ;
  lemon:sense          :party-5695_sense_1 ;
  lemon:synBehavior    :party-5695_nounpp .

:party-5695_form a lemon:Form ;
  lemon:writtenRep "party"@en .

:party-5695_singular a    lemon:Form ;
  lemon:writtenRep "party"@en ;
  lexinfo:number   lexinfo:singular .

:party-5695_plural a   lemon:Form ;
  lemon:writtenRep "party"@en ;
  lexinfo:number    lexinfo:plural .

:party-5695_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:party-5695_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :party-5695_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/party> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :party-5695_sense_1_condition .

:party-5695_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/PoliticalParty> .

:arg2 lemon:marker :party-5695_form_preposition .
## Prepositions ##
:party-5695_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

