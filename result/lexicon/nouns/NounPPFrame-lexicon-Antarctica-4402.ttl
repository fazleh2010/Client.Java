@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Antarctica-4402 ;
  lemon:entry    :Antarctica-4402_form_preposition .

:Antarctica-4402 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Antarctica-4402_form ;
  lemon:otherForm  :Antarctica-4402_singular ;
  lemon:otherForm  :Antarctica-4402_plural ;
  lemon:sense          :Antarctica-4402_sense_1 ;
  lemon:synBehavior    :Antarctica-4402_nounpp .

:Antarctica-4402_form a lemon:Form ;
  lemon:writtenRep "Antarctica"@en .

:Antarctica-4402_singular a    lemon:Form ;
  lemon:writtenRep "Antarctica"@en ;
  lexinfo:number   lexinfo:singular .

:Antarctica-4402_plural a   lemon:Form ;
  lemon:writtenRep "Antarctica"@en ;
  lexinfo:number    lexinfo:plural .

:Antarctica-4402_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Antarctica-4402_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Antarctica-4402_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/locatedInArea> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Antarctica-4402_sense_1_condition .

:Antarctica-4402_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :Antarctica-4402_form_preposition .
## Prepositions ##
:Antarctica-4402_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

