@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :theatre-117 ;
  lemon:entry    :theatre-117_form_preposition .

:theatre-117 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :theatre-117_form ;
  lemon:otherForm  :theatre-117_singular ;
  lemon:otherForm  :theatre-117_plural ;
  lemon:sense          :theatre-117_sense_1 ;
  lemon:synBehavior    :theatre-117_nounpp .

:theatre-117_form a lemon:Form ;
