:theatre-117_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:theatre-117_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
