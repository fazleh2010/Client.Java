@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :speed-1888 ;
  lemon:entry    :speed-1888_form_preposition .

:speed-1888 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :speed-1888_form ;
  lemon:otherForm  :speed-1888_singular ;
  lemon:otherForm  :speed-1888_plural ;
  lemon:sense          :speed-1888_sense_1 ;
  lemon:synBehavior    :speed-1888_nounpp .

:speed-1888_form a lemon:Form ;
  lemon:writtenRep "speed"@en .

:speed-1888_singular a    lemon:Form ;
  lemon:writtenRep "speed"@en ;
  lexinfo:number   lexinfo:singular .

:speed-1888_plural a   lemon:Form ;
  lemon:writtenRep "speed"@en ;
  lexinfo:number    lexinfo:plural .

:speed-1888_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:speed-1888_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :speed-1888_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/deathDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :speed-1888_sense_1_condition .

:speed-1888_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :speed-1888_form_preposition .
## Prepositions ##
:speed-1888_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

