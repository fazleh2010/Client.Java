@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :James-5240 ;
  lemon:entry    :James-5240_form_preposition .

:James-5240 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :James-5240_form ;
  lemon:otherForm  :James-5240_singular ;
  lemon:otherForm  :James-5240_plural ;
  lemon:sense          :James-5240_sense_1 ;
  lemon:synBehavior    :James-5240_nounpp .

:James-5240_form a lemon:Form ;
  lemon:writtenRep "James"@en .

:James-5240_singular a    lemon:Form ;
  lemon:writtenRep "James"@en ;
  lexinfo:number   lexinfo:singular .

:James-5240_plural a   lemon:Form ;
  lemon:writtenRep "James"@en ;
  lexinfo:number    lexinfo:plural .

:James-5240_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:James-5240_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :James-5240_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/numberOfPages> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :James-5240_sense_1_condition .

:James-5240_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/WrittenWork> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#positiveInteger> .

:arg2 lemon:marker :James-5240_form_preposition .
## Prepositions ##
:James-5240_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

