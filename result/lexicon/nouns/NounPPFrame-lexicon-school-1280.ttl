@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :school-1280 ;
  lemon:entry    :school-1280_form_preposition .

:school-1280 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :school-1280_form ;
  lemon:otherForm  :school-1280_singular ;
  lemon:otherForm  :school-1280_plural ;
  lemon:sense          :school-1280_sense_1 ;
  lemon:synBehavior    :school-1280_nounpp .

:school-1280_form a lemon:Form ;
  lemon:writtenRep "school"@en .

:school-1280_singular a    lemon:Form ;
  lemon:writtenRep "school"@en ;
  lexinfo:number   lexinfo:singular .

:school-1280_plural a   lemon:Form ;
  lemon:writtenRep "school"@en ;
  lexinfo:number    lexinfo:plural .

:school-1280_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:school-1280_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :school-1280_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/city> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :school-1280_sense_1_condition .

:school-1280_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :school-1280_form_preposition .
## Prepositions ##
:school-1280_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

