@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :place-464 ;
  lemon:entry    :place-464_form_preposition .

:place-464 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :place-464_form ;
  lemon:otherForm  :place-464_singular ;
  lemon:otherForm  :place-464_plural ;
  lemon:sense          :place-464_sense_1 ;
  lemon:synBehavior    :place-464_nounpp .

:place-464_form a lemon:Form ;
  lemon:writtenRep "place"@en .

:place-464_singular a    lemon:Form ;
  lemon:writtenRep "place"@en ;
  lexinfo:number   lexinfo:singular .

:place-464_plural a   lemon:Form ;
  lemon:writtenRep "place"@en ;
  lexinfo:number    lexinfo:plural .

:place-464_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:place-464_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :place-464_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/areaTotal> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :place-464_sense_1_condition .

:place-464_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :place-464_form_preposition .
## Prepositions ##
:place-464_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

