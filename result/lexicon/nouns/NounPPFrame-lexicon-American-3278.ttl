@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :American-3278 ;
  lemon:entry    :American-3278_form_preposition .

:American-3278 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :American-3278_form ;
  lemon:otherForm  :American-3278_singular ;
  lemon:otherForm  :American-3278_plural ;
  lemon:sense          :American-3278_sense_1 ;
  lemon:synBehavior    :American-3278_nounpp .

:American-3278_form a lemon:Form ;
  lemon:writtenRep "American"@en .

:American-3278_singular a    lemon:Form ;
  lemon:writtenRep "American"@en ;
  lexinfo:number   lexinfo:singular .

:American-3278_plural a   lemon:Form ;
  lemon:writtenRep "American"@en ;
  lexinfo:number    lexinfo:plural .

:American-3278_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:American-3278_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :American-3278_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/growingGrape> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :American-3278_sense_1_condition .

:American-3278_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/WineRegion> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Grape> .

:arg2 lemon:marker :American-3278_form_preposition .
## Prepositions ##
:American-3278_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

