@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :theater-120 ;
  lemon:entry    :theater-120_form_preposition .

:theater-120 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :theater-120_form ;
  lemon:otherForm  :theater-120_singular ;
  lemon:otherForm  :theater-120_plural ;
  lemon:sense          :theater-120_sense_1 ;
  lemon:synBehavior    :theater-120_nounpp .

:theater-120_form a lemon:Form ;
  lemon:writtenRep "theater"@en .

:theater-120_singular a    lemon:Form ;
  lemon:writtenRep "theater"@en ;
  lexinfo:number   lexinfo:singular .

:theater-120_plural a   lemon:Form ;
  lemon:writtenRep "theater"@en ;
  lexinfo:number    lexinfo:plural .

:theater-120_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:theater-120_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :theater-120_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/address> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :theater-120_sense_1_condition .

:theater-120_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :theater-120_form_preposition .
## Prepositions ##
:theater-120_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

