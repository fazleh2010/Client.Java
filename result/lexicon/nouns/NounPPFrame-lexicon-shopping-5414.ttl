@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :shopping-5414 ;
  lemon:entry    :shopping-5414_form_preposition .

:shopping-5414 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :shopping-5414_form ;
  lemon:otherForm  :shopping-5414_singular ;
  lemon:otherForm  :shopping-5414_plural ;
  lemon:sense          :shopping-5414_sense_1 ;
  lemon:synBehavior    :shopping-5414_nounpp .

:shopping-5414_form a lemon:Form ;
  lemon:writtenRep "shopping"@en .

:shopping-5414_singular a    lemon:Form ;
  lemon:writtenRep "shopping"@en ;
  lexinfo:number   lexinfo:singular .

:shopping-5414_plural a   lemon:Form ;
  lemon:writtenRep "shopping"@en ;
  lexinfo:number    lexinfo:plural .

:shopping-5414_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:shopping-5414_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :shopping-5414_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/openingDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :shopping-5414_sense_1_condition .

:shopping-5414_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :shopping-5414_form_preposition .
## Prepositions ##
:shopping-5414_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

