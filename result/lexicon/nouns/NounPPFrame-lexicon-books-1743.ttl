@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :books-1743 ;
  lemon:entry    :books-1743_form_preposition .

:books-1743 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :books-1743_form ;
  lemon:otherForm  :books-1743_singular ;
  lemon:otherForm  :books-1743_plural ;
  lemon:sense          :books-1743_sense_1 ;
  lemon:synBehavior    :books-1743_nounpp .

:books-1743_form a lemon:Form ;
  lemon:writtenRep "books"@en .

:books-1743_singular a    lemon:Form ;
  lemon:writtenRep "books"@en ;
  lexinfo:number   lexinfo:singular .

:books-1743_plural a   lemon:Form ;
  lemon:writtenRep "books"@en ;
  lexinfo:number    lexinfo:plural .

:books-1743_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:books-1743_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :books-1743_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/creator> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :books-1743_sense_1_condition .

:books-1743_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Agent> .

:arg2 lemon:marker :books-1743_form_preposition .
## Prepositions ##
:books-1743_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

