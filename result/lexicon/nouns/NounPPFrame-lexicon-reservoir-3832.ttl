@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :reservoir-3832 ;
  lemon:entry    :reservoir-3832_form_preposition .

:reservoir-3832 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :reservoir-3832_form ;
  lemon:otherForm  :reservoir-3832_singular ;
  lemon:otherForm  :reservoir-3832_plural ;
  lemon:sense          :reservoir-3832_sense_1 ;
  lemon:synBehavior    :reservoir-3832_nounpp .

:reservoir-3832_form a lemon:Form ;
  lemon:writtenRep "reservoir"@en .

:reservoir-3832_singular a    lemon:Form ;
  lemon:writtenRep "reservoir"@en ;
  lexinfo:number   lexinfo:singular .

:reservoir-3832_plural a   lemon:Form ;
  lemon:writtenRep "reservoir"@en ;
  lexinfo:number    lexinfo:plural .

:reservoir-3832_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:reservoir-3832_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :reservoir-3832_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/inflow> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :reservoir-3832_sense_1_condition .

:reservoir-3832_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/River> .

:arg2 lemon:marker :reservoir-3832_form_preposition .
## Prepositions ##
:reservoir-3832_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

