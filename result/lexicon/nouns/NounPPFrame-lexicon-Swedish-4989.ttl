@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Swedish-4989 ;
  lemon:entry    :Swedish-4989_form_preposition .

:Swedish-4989 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Swedish-4989_form ;
  lemon:otherForm  :Swedish-4989_singular ;
  lemon:otherForm  :Swedish-4989_plural ;
  lemon:sense          :Swedish-4989_sense_1 ;
  lemon:synBehavior    :Swedish-4989_nounpp .

:Swedish-4989_form a lemon:Form ;
  lemon:writtenRep "Swedish"@en .

:Swedish-4989_singular a    lemon:Form ;
  lemon:writtenRep "Swedish"@en ;
  lexinfo:number   lexinfo:singular .

:Swedish-4989_plural a   lemon:Form ;
  lemon:writtenRep "Swedish"@en ;
  lexinfo:number    lexinfo:plural .

:Swedish-4989_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Swedish-4989_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Swedish-4989_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/nationality> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Swedish-4989_sense_1_condition .

:Swedish-4989_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :Swedish-4989_form_preposition .
## Prepositions ##
:Swedish-4989_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

