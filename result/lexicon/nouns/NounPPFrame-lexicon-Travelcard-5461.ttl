@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Travelcard-5461 ;
  lemon:entry    :Travelcard-5461_form_preposition .

:Travelcard-5461 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Travelcard-5461_form ;
  lemon:otherForm  :Travelcard-5461_singular ;
  lemon:otherForm  :Travelcard-5461_plural ;
  lemon:sense          :Travelcard-5461_sense_1 ;
  lemon:synBehavior    :Travelcard-5461_nounpp .

:Travelcard-5461_form a lemon:Form ;
  lemon:writtenRep "Travelcard"@en .

:Travelcard-5461_singular a    lemon:Form ;
  lemon:writtenRep "Travelcard"@en ;
  lexinfo:number   lexinfo:singular .

:Travelcard-5461_plural a   lemon:Form ;
  lemon:writtenRep "Travelcard"@en ;
  lexinfo:number    lexinfo:plural .

:Travelcard-5461_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Travelcard-5461_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Travelcard-5461_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/operator> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Travelcard-5461_sense_1_condition .

:Travelcard-5461_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/LaunchPad> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Organisation> .

:arg2 lemon:marker :Travelcard-5461_form_preposition .
## Prepositions ##
:Travelcard-5461_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

