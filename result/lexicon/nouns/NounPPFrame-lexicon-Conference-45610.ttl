@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Conference-45610 ;
  lemon:entry    :Conference-45610_form_preposition .

:Conference-45610 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Conference-45610_form ;
  lemon:otherForm  :Conference-45610_singular ;
  lemon:otherForm  :Conference-45610_plural ;
  lemon:sense          :Conference-45610_sense_1 ;
  lemon:synBehavior    :Conference-45610_nounpp .

:Conference-45610_form a lemon:Form ;
  lemon:writtenRep "Conference"@en .

:Conference-45610_singular a    lemon:Form ;
  lemon:writtenRep "Conference"@en ;
  lexinfo:number   lexinfo:singular .

:Conference-45610_plural a   lemon:Form ;
  lemon:writtenRep "Conference"@en ;
  lexinfo:number    lexinfo:plural .

:Conference-45610_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Conference-45610_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Conference-45610_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/nickname> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Conference-45610_sense_1_condition .

:Conference-45610_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#String> .

:arg2 lemon:marker :Conference-45610_form_preposition .
## Prepositions ##
:Conference-45610_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

