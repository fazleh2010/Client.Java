@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :film-2078 ;
  lemon:entry    :film-2078_form_preposition .

:film-2078 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :film-2078_form ;
  lemon:otherForm  :film-2078_singular ;
  lemon:otherForm  :film-2078_plural ;
  lemon:sense          :film-2078_sense_1 ;
  lemon:synBehavior    :film-2078_nounpp .

:film-2078_form a lemon:Form ;
  lemon:writtenRep "film"@en .

:film-2078_singular a    lemon:Form ;
  lemon:writtenRep "film"@en ;
  lexinfo:number   lexinfo:singular .

:film-2078_plural a   lemon:Form ;
  lemon:writtenRep "film"@en ;
  lexinfo:number    lexinfo:plural .

:film-2078_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:film-2078_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :film-2078_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/director> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :film-2078_sense_1_condition .

:film-2078_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :film-2078_form_preposition .
## Prepositions ##
:film-2078_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

