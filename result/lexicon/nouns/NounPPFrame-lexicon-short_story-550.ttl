@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :short_story-550 ;
  lemon:entry    :short_story-550_form_preposition .

:short_story-550 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :short_story-550_form ;
  lemon:otherForm  :short_story-550_singular ;
  lemon:otherForm  :short_story-550_plural ;
  lemon:sense          :short_story-550_sense_1 ;
  lemon:synBehavior    :short_story-550_nounpp .

:short_story-550_form a lemon:Form ;
  lemon:writtenRep "short story"@en .

:short_story-550_singular a    lemon:Form ;
  lemon:writtenRep "short story"@en ;
  lexinfo:number   lexinfo:singular .

:short_story-550_plural a   lemon:Form ;
  lemon:writtenRep "short story"@en ;
  lexinfo:number    lexinfo:plural .

:short_story-550_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:short_story-550_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :short_story-550_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/author> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :short_story-550_sense_1_condition .

:short_story-550_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Book> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :short_story-550_form_preposition .
## Prepositions ##
:short_story-550_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

