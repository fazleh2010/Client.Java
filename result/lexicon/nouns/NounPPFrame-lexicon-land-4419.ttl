@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :land-4419 ;
  lemon:entry    :land-4419_form_preposition .

:land-4419 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :land-4419_form ;
  lemon:otherForm  :land-4419_singular ;
  lemon:otherForm  :land-4419_plural ;
  lemon:sense          :land-4419_sense_1 ;
  lemon:synBehavior    :land-4419_nounpp .

:land-4419_form a lemon:Form ;
  lemon:writtenRep "land"@en .

:land-4419_singular a    lemon:Form ;
  lemon:writtenRep "land"@en ;
  lexinfo:number   lexinfo:singular .

:land-4419_plural a   lemon:Form ;
  lemon:writtenRep "land"@en ;
  lexinfo:number    lexinfo:plural .

:land-4419_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:land-4419_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :land-4419_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/location> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :land-4419_sense_1_condition .

:land-4419_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Building> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :land-4419_form_preposition .
## Prepositions ##
:land-4419_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

