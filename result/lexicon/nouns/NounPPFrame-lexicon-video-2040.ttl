@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :video-2040 ;
  lemon:entry    :video-2040_form_preposition .

:video-2040 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :video-2040_form ;
  lemon:otherForm  :video-2040_singular ;
  lemon:otherForm  :video-2040_plural ;
  lemon:sense          :video-2040_sense_1 ;
  lemon:synBehavior    :video-2040_nounpp .

:video-2040_form a lemon:Form ;
  lemon:writtenRep "video"@en .

:video-2040_singular a    lemon:Form ;
  lemon:writtenRep "video"@en ;
  lexinfo:number   lexinfo:singular .

:video-2040_plural a   lemon:Form ;
  lemon:writtenRep "video"@en ;
  lexinfo:number    lexinfo:plural .

:video-2040_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:video-2040_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :video-2040_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/developer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :video-2040_sense_1_condition .

:video-2040_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/VideoGame> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Agent> .

:arg2 lemon:marker :video-2040_form_preposition .
## Prepositions ##
:video-2040_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

