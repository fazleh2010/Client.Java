@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :book-554 ;
  lemon:entry    :book-554_form_preposition .

:book-554 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :book-554_form ;
  lemon:otherForm  :book-554_singular ;
  lemon:otherForm  :book-554_plural ;
  lemon:sense          :book-554_sense_1 ;
  lemon:synBehavior    :book-554_nounpp .

:book-554_form a lemon:Form ;
  lemon:writtenRep "book"@en .

:book-554_singular a    lemon:Form ;
  lemon:writtenRep "book"@en ;
  lexinfo:number   lexinfo:singular .

:book-554_plural a   lemon:Form ;
  lemon:writtenRep "book"@en ;
  lexinfo:number    lexinfo:plural .

:book-554_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:book-554_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :book-554_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/author> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :book-554_sense_1_condition .

:book-554_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Book> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :book-554_form_preposition .
## Prepositions ##
:book-554_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

