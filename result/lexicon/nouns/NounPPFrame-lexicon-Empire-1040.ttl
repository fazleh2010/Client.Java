@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Empire-1040 ;
  lemon:entry    :Empire-1040_form_preposition .

:Empire-1040 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Empire-1040_form ;
  lemon:otherForm  :Empire-1040_singular ;
  lemon:otherForm  :Empire-1040_plural ;
  lemon:sense          :Empire-1040_sense_1 ;
  lemon:synBehavior    :Empire-1040_nounpp .

:Empire-1040_form a lemon:Form ;
  lemon:writtenRep "Empire"@en .

:Empire-1040_singular a    lemon:Form ;
  lemon:writtenRep "Empire"@en ;
  lexinfo:number   lexinfo:singular .

:Empire-1040_plural a   lemon:Form ;
  lemon:writtenRep "Empire"@en ;
  lexinfo:number    lexinfo:plural .

:Empire-1040_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Empire-1040_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Empire-1040_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/capital> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Empire-1040_sense_1_condition .

:Empire-1040_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :Empire-1040_form_preposition .
## Prepositions ##
:Empire-1040_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

