@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :airline_based-3325 ;
  lemon:entry    :airline_based-3325_form_preposition .

:airline_based-3325 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :airline_based-3325_form ;
  lemon:otherForm  :airline_based-3325_singular ;
  lemon:otherForm  :airline_based-3325_plural ;
  lemon:sense          :airline_based-3325_sense_1 ;
  lemon:synBehavior    :airline_based-3325_nounpp .

:airline_based-3325_form a lemon:Form ;
  lemon:writtenRep "airline based"@en .

:airline_based-3325_singular a    lemon:Form ;
  lemon:writtenRep "airline based"@en ;
  lexinfo:number   lexinfo:singular .

:airline_based-3325_plural a   lemon:Form ;
  lemon:writtenRep "airline based"@en ;
  lexinfo:number    lexinfo:plural .

:airline_based-3325_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:airline_based-3325_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :airline_based-3325_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/headquarter> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :airline_based-3325_sense_1_condition .

:airline_based-3325_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/PopulatedPlace> .

:arg2 lemon:marker :airline_based-3325_form_preposition .
## Prepositions ##
:airline_based-3325_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

