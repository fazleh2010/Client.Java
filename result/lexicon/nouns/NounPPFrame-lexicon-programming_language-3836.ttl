@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :programming_language-3836 ;
  lemon:entry    :programming_language-3836_form_preposition .

:programming_language-3836 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :programming_language-3836_form ;
  lemon:otherForm  :programming_language-3836_singular ;
  lemon:otherForm  :programming_language-3836_plural ;
  lemon:sense          :programming_language-3836_sense_1 ;
  lemon:synBehavior    :programming_language-3836_nounpp .

:programming_language-3836_form a lemon:Form ;
  lemon:writtenRep "programming language"@en .

:programming_language-3836_singular a    lemon:Form ;
  lemon:writtenRep "programming language"@en ;
  lexinfo:number   lexinfo:singular .

:programming_language-3836_plural a   lemon:Form ;
  lemon:writtenRep "programming language"@en ;
  lexinfo:number    lexinfo:plural .

:programming_language-3836_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:programming_language-3836_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :programming_language-3836_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/influenced> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :programming_language-3836_sense_1_condition .

:programming_language-3836_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :programming_language-3836_form_preposition .
## Prepositions ##
:programming_language-3836_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

