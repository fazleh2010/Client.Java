@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :populated_place-2523 ;
  lemon:entry    :populated_place-2523_form_preposition .

:populated_place-2523 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :populated_place-2523_form ;
  lemon:otherForm  :populated_place-2523_singular ;
  lemon:otherForm  :populated_place-2523_plural ;
  lemon:sense          :populated_place-2523_sense_1 ;
  lemon:synBehavior    :populated_place-2523_nounpp .

:populated_place-2523_form a lemon:Form ;
  lemon:writtenRep "populated place"@en .

:populated_place-2523_singular a    lemon:Form ;
  lemon:writtenRep "populated place"@en ;
  lexinfo:number   lexinfo:singular .

:populated_place-2523_plural a   lemon:Form ;
  lemon:writtenRep "populated place"@en ;
  lexinfo:number    lexinfo:plural .

:populated_place-2523_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:populated_place-2523_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :populated_place-2523_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/elevation> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :populated_place-2523_sense_1_condition .

:populated_place-2523_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :populated_place-2523_form_preposition .
## Prepositions ##
:populated_place-2523_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

