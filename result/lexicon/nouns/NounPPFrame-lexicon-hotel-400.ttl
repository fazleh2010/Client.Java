@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :hotel-400 ;
  lemon:entry    :hotel-400_form_preposition .

:hotel-400 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :hotel-400_form ;
  lemon:otherForm  :hotel-400_singular ;
  lemon:otherForm  :hotel-400_plural ;
  lemon:sense          :hotel-400_sense_1 ;
  lemon:synBehavior    :hotel-400_nounpp .

:hotel-400_form a lemon:Form ;
  lemon:writtenRep "hotel"@en .

:hotel-400_singular a    lemon:Form ;
  lemon:writtenRep "hotel"@en ;
  lexinfo:number   lexinfo:singular .

:hotel-400_plural a   lemon:Form ;
  lemon:writtenRep "hotel"@en ;
  lexinfo:number    lexinfo:plural .

:hotel-400_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:hotel-400_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :hotel-400_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/architect> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :hotel-400_sense_1_condition .

:hotel-400_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Architect> .

:arg2 lemon:marker :hotel-400_form_preposition .
## Prepositions ##
:hotel-400_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

