@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :killer-1877 ;
  lemon:entry    :killer-1877_form_preposition .

:killer-1877 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :killer-1877_form ;
  lemon:otherForm  :killer-1877_singular ;
  lemon:otherForm  :killer-1877_plural ;
  lemon:sense          :killer-1877_sense_1 ;
  lemon:synBehavior    :killer-1877_nounpp .

:killer-1877_form a lemon:Form ;
  lemon:writtenRep "killer"@en .

:killer-1877_singular a    lemon:Form ;
  lemon:writtenRep "killer"@en ;
  lexinfo:number   lexinfo:singular .

:killer-1877_plural a   lemon:Form ;
  lemon:writtenRep "killer"@en ;
  lexinfo:number    lexinfo:plural .

:killer-1877_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:killer-1877_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :killer-1877_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/deathCause> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :killer-1877_sense_1_condition .

:killer-1877_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2002/07/owl#Thing> .

:arg2 lemon:marker :killer-1877_form_preposition .
## Prepositions ##
:killer-1877_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

