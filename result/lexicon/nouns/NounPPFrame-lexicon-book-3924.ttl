@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :book-3924 ;
  lemon:entry    :book-3924_form_preposition .

:book-3924 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :book-3924_form ;
  lemon:otherForm  :book-3924_singular ;
  lemon:otherForm  :book-3924_plural ;
  lemon:sense          :book-3924_sense_1 ;
  lemon:synBehavior    :book-3924_nounpp .

:book-3924_form a lemon:Form ;
  lemon:writtenRep "book"@en .

:book-3924_singular a    lemon:Form ;
  lemon:writtenRep "book"@en ;
  lexinfo:number   lexinfo:singular .

:book-3924_plural a   lemon:Form ;
  lemon:writtenRep "book"@en ;
  lexinfo:number    lexinfo:plural .

:book-3924_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:book-3924_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :book-3924_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/isbn> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :book-3924_sense_1_condition .

:book-3924_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Book> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :book-3924_form_preposition .
## Prepositions ##
:book-3924_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

