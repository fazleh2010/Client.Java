@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :United-1923 ;
  lemon:entry    :United-1923_form_preposition .

:United-1923 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :United-1923_form ;
  lemon:otherForm  :United-1923_singular ;
  lemon:otherForm  :United-1923_plural ;
  lemon:sense          :United-1923_sense_1 ;
  lemon:synBehavior    :United-1923_nounpp .

:United-1923_form a lemon:Form ;
  lemon:writtenRep "United"@en .

:United-1923_singular a    lemon:Form ;
  lemon:writtenRep "United"@en ;
  lexinfo:number   lexinfo:singular .

:United-1923_plural a   lemon:Form ;
  lemon:writtenRep "United"@en ;
  lexinfo:number    lexinfo:plural .

:United-1923_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:United-1923_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :United-1923_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/deathPlace> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :United-1923_sense_1_condition .

:United-1923_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Politician> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :United-1923_form_preposition .
## Prepositions ##
:United-1923_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

