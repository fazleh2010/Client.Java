@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :census-465 ;
  lemon:entry    :census-465_form_preposition .

:census-465 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :census-465_form ;
  lemon:otherForm  :census-465_singular ;
  lemon:otherForm  :census-465_plural ;
  lemon:sense          :census-465_sense_1 ;
  lemon:synBehavior    :census-465_nounpp .

:census-465_form a lemon:Form ;
  lemon:writtenRep "census"@en .

:census-465_singular a    lemon:Form ;
  lemon:writtenRep "census"@en ;
  lexinfo:number   lexinfo:singular .

:census-465_plural a   lemon:Form ;
  lemon:writtenRep "census"@en ;
  lexinfo:number    lexinfo:plural .

:census-465_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:census-465_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :census-465_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/areaTotal> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :census-465_sense_1_condition .

:census-465_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :census-465_form_preposition .
## Prepositions ##
:census-465_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

