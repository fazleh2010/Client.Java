@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :journal-12 ;
  lemon:entry    :journal-12_form_preposition .

:journal-12 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :journal-12_form ;
  lemon:otherForm  :journal-12_singular ;
  lemon:otherForm  :journal-12_plural ;
  lemon:sense          :journal-12_sense_1 ;
  lemon:synBehavior    :journal-12_nounpp .

:journal-12_form a lemon:Form ;
  lemon:writtenRep "journal"@en .

:journal-12_singular a    lemon:Form ;
  lemon:writtenRep "journal"@en ;
  lexinfo:number   lexinfo:singular .

:journal-12_plural a   lemon:Form ;
  lemon:writtenRep "journal"@en ;
  lexinfo:number    lexinfo:plural .

:journal-12_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:journal-12_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :journal-12_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/abbreviation> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :journal-12_sense_1_condition .

:journal-12_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://www.w3.org/2002/07/owl#Thing> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :journal-12_form_preposition .
## Prepositions ##
:journal-12_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

