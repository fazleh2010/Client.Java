@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :loch-5730 ;
  lemon:entry    :loch-5730_form_preposition .

:loch-5730 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :loch-5730_form ;
  lemon:otherForm  :loch-5730_singular ;
  lemon:otherForm  :loch-5730_plural ;
  lemon:sense          :loch-5730_sense_1 ;
  lemon:synBehavior    :loch-5730_nounpp .

:loch-5730_form a lemon:Form ;
  lemon:writtenRep "loch"@en .

:loch-5730_singular a    lemon:Form ;
  lemon:writtenRep "loch"@en ;
  lexinfo:number   lexinfo:singular .

:loch-5730_plural a   lemon:Form ;
  lemon:writtenRep "loch"@en ;
  lexinfo:number    lexinfo:plural .

:loch-5730_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:loch-5730_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :loch-5730_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/minimumTemperature> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :loch-5730_sense_1_condition .

:loch-5730_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Planet> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :loch-5730_form_preposition .
## Prepositions ##
:loch-5730_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

