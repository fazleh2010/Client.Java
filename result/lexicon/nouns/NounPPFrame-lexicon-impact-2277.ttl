@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :impact-2277 ;
  lemon:entry    :impact-2277_form_preposition .

:impact-2277 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :impact-2277_form ;
  lemon:otherForm  :impact-2277_singular ;
  lemon:otherForm  :impact-2277_plural ;
  lemon:sense          :impact-2277_sense_1 ;
  lemon:synBehavior    :impact-2277_nounpp .

:impact-2277_form a lemon:Form ;
  lemon:writtenRep "impact"@en .

:impact-2277_singular a    lemon:Form ;
  lemon:writtenRep "impact"@en ;
  lexinfo:number   lexinfo:singular .

:impact-2277_plural a   lemon:Form ;
  lemon:writtenRep "impact"@en ;
  lexinfo:number    lexinfo:plural .

:impact-2277_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:impact-2277_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :impact-2277_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/diameter> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :impact-2277_sense_1_condition .

:impact-2277_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Device> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :impact-2277_form_preposition .
## Prepositions ##
:impact-2277_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

