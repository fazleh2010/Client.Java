@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :American-576 ;
  lemon:entry    :American-576_form_preposition .

:American-576 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :American-576_form ;
  lemon:otherForm  :American-576_singular ;
  lemon:otherForm  :American-576_plural ;
  lemon:sense          :American-576_sense_1 ;
  lemon:synBehavior    :American-576_nounpp .

:American-576_form a lemon:Form ;
  lemon:writtenRep "American"@en .

:American-576_singular a    lemon:Form ;
  lemon:writtenRep "American"@en ;
  lexinfo:number   lexinfo:singular .

:American-576_plural a   lemon:Form ;
  lemon:writtenRep "American"@en ;
  lexinfo:number    lexinfo:plural .

:American-576_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:American-576_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :American-576_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/award> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :American-576_sense_1_condition .

:American-576_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Scientist> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Award> .

:arg2 lemon:marker :American-576_form_preposition .
## Prepositions ##
:American-576_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

