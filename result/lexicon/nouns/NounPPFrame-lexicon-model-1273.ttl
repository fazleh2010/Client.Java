@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :model-1273 ;
  lemon:entry    :model-1273_form_preposition .

:model-1273 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :model-1273_form ;
  lemon:otherForm  :model-1273_singular ;
  lemon:otherForm  :model-1273_plural ;
  lemon:sense          :model-1273_sense_1 ;
  lemon:synBehavior    :model-1273_nounpp .

:model-1273_form a lemon:Form ;
  lemon:writtenRep "model"@en .

:model-1273_singular a    lemon:Form ;
  lemon:writtenRep "model"@en ;
  lexinfo:number   lexinfo:singular .

:model-1273_plural a   lemon:Form ;
  lemon:writtenRep "model"@en ;
  lexinfo:number    lexinfo:plural .

:model-1273_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:model-1273_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :model-1273_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/citizenship> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :model-1273_sense_1_condition .

:model-1273_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :model-1273_form_preposition .
## Prepositions ##
:model-1273_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

