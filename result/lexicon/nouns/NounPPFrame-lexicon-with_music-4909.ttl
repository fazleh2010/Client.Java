@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :with_music-4909 ;
  lemon:entry    :with_music-4909_form_preposition .

:with_music-4909 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :with_music-4909_form ;
  lemon:otherForm  :with_music-4909_singular ;
  lemon:otherForm  :with_music-4909_plural ;
  lemon:sense          :with_music-4909_sense_1 ;
  lemon:synBehavior    :with_music-4909_nounpp .

:with_music-4909_form a lemon:Form ;
  lemon:writtenRep "with music"@en .

:with_music-4909_singular a    lemon:Form ;
  lemon:writtenRep "with music"@en ;
  lexinfo:number   lexinfo:singular .

:with_music-4909_plural a   lemon:Form ;
  lemon:writtenRep "with music"@en ;
  lexinfo:number    lexinfo:plural .

:with_music-4909_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:with_music-4909_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :with_music-4909_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/musicBy> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :with_music-4909_sense_1_condition .

:with_music-4909_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Musical> .

:arg2 lemon:marker :with_music-4909_form_preposition .
## Prepositions ##
:with_music-4909_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

