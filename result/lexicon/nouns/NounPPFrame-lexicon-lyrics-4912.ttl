@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lyrics-4912 ;
  lemon:entry    :lyrics-4912_form_preposition .

:lyrics-4912 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lyrics-4912_form ;
  lemon:otherForm  :lyrics-4912_singular ;
  lemon:otherForm  :lyrics-4912_plural ;
  lemon:sense          :lyrics-4912_sense_1 ;
  lemon:synBehavior    :lyrics-4912_nounpp .

:lyrics-4912_form a lemon:Form ;
  lemon:writtenRep "lyrics"@en .

:lyrics-4912_singular a    lemon:Form ;
  lemon:writtenRep "lyrics"@en ;
  lexinfo:number   lexinfo:singular .

:lyrics-4912_plural a   lemon:Form ;
  lemon:writtenRep "lyrics"@en ;
  lexinfo:number    lexinfo:plural .

:lyrics-4912_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:lyrics-4912_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lyrics-4912_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/musicBy> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :lyrics-4912_sense_1_condition .

:lyrics-4912_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Musical> .

:arg2 lemon:marker :lyrics-4912_form_preposition .
## Prepositions ##
:lyrics-4912_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

