@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Mark-6585 ;
  lemon:entry    :Mark-6585_form_preposition .

:Mark-6585 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Mark-6585_form ;
  lemon:otherForm  :Mark-6585_singular ;
  lemon:otherForm  :Mark-6585_plural ;
  lemon:sense          :Mark-6585_sense_1 ;
  lemon:synBehavior    :Mark-6585_nounpp .

:Mark-6585_form a lemon:Form ;
  lemon:writtenRep "Mark"@en .

:Mark-6585_singular a    lemon:Form ;
  lemon:writtenRep "Mark"@en ;
  lexinfo:number   lexinfo:singular .

:Mark-6585_plural a   lemon:Form ;
  lemon:writtenRep "Mark"@en ;
  lexinfo:number    lexinfo:plural .

:Mark-6585_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Mark-6585_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Mark-6585_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/residence> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Mark-6585_sense_1_condition .

:Mark-6585_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Single> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :Mark-6585_form_preposition .
## Prepositions ##
:Mark-6585_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

