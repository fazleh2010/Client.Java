@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lake-3827 ;
  lemon:entry    :lake-3827_form_preposition .

:lake-3827 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lake-3827_form ;
  lemon:otherForm  :lake-3827_singular ;
  lemon:otherForm  :lake-3827_plural ;
  lemon:sense          :lake-3827_sense_1 ;
  lemon:synBehavior    :lake-3827_nounpp .

:lake-3827_form a lemon:Form ;
  lemon:writtenRep "lake"@en .

:lake-3827_singular a    lemon:Form ;
  lemon:writtenRep "lake"@en ;
  lexinfo:number   lexinfo:singular .

:lake-3827_plural a   lemon:Form ;
  lemon:writtenRep "lake"@en ;
  lexinfo:number    lexinfo:plural .

:lake-3827_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:lake-3827_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lake-3827_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/inflow> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :lake-3827_sense_1_condition .

:lake-3827_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/River> .

:arg2 lemon:marker :lake-3827_form_preposition .
## Prepositions ##
:lake-3827_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

