@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Museum-4901 ;
  lemon:entry    :Museum-4901_form_preposition .

:Museum-4901 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Museum-4901_form ;
  lemon:otherForm  :Museum-4901_singular ;
  lemon:otherForm  :Museum-4901_plural ;
  lemon:sense          :Museum-4901_sense_1 ;
  lemon:synBehavior    :Museum-4901_nounpp .

:Museum-4901_form a lemon:Form ;
  lemon:writtenRep "Museum"@en .

:Museum-4901_singular a    lemon:Form ;
  lemon:writtenRep "Museum"@en ;
  lexinfo:number   lexinfo:singular .

:Museum-4901_plural a   lemon:Form ;
  lemon:writtenRep "Museum"@en ;
  lexinfo:number    lexinfo:plural .

:Museum-4901_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Museum-4901_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Museum-4901_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/museum> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Museum-4901_sense_1_condition .

:Museum-4901_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Artwork> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Museum> .

:arg2 lemon:marker :Museum-4901_form_preposition .
## Prepositions ##
:Museum-4901_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

