@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :radio-1282 ;
  lemon:entry    :radio-1282_form_preposition .

:radio-1282 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :radio-1282_form ;
  lemon:otherForm  :radio-1282_singular ;
  lemon:otherForm  :radio-1282_plural ;
  lemon:sense          :radio-1282_sense_1 ;
  lemon:synBehavior    :radio-1282_nounpp .

:radio-1282_form a lemon:Form ;
  lemon:writtenRep "radio"@en .

:radio-1282_singular a    lemon:Form ;
  lemon:writtenRep "radio"@en ;
  lexinfo:number   lexinfo:singular .

:radio-1282_plural a   lemon:Form ;
  lemon:writtenRep "radio"@en ;
  lexinfo:number    lexinfo:plural .

:radio-1282_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:radio-1282_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :radio-1282_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/city> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :radio-1282_sense_1_condition .

:radio-1282_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :radio-1282_form_preposition .
## Prepositions ##
:radio-1282_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

