@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :University-1296 ;
  lemon:entry    :University-1296_form_preposition .

:University-1296 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :University-1296_form ;
  lemon:otherForm  :University-1296_singular ;
  lemon:otherForm  :University-1296_plural ;
  lemon:sense          :University-1296_sense_1 ;
  lemon:synBehavior    :University-1296_nounpp .

:University-1296_form a lemon:Form ;
  lemon:writtenRep "University"@en .

:University-1296_singular a    lemon:Form ;
  lemon:writtenRep "University"@en ;
  lexinfo:number   lexinfo:singular .

:University-1296_plural a   lemon:Form ;
  lemon:writtenRep "University"@en ;
  lexinfo:number    lexinfo:plural .

:University-1296_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:University-1296_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :University-1296_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/city> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :University-1296_sense_1_condition .

:University-1296_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :University-1296_form_preposition .
## Prepositions ##
:University-1296_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

