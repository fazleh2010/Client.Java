@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Football-1411 ;
  lemon:entry    :Football-1411_form_preposition .

:Football-1411 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Football-1411_form ;
  lemon:otherForm  :Football-1411_singular ;
  lemon:otherForm  :Football-1411_plural ;
  lemon:sense          :Football-1411_sense_1 ;
  lemon:synBehavior    :Football-1411_nounpp .

:Football-1411_form a lemon:Form ;
  lemon:writtenRep "Football"@en .

:Football-1411_singular a    lemon:Form ;
  lemon:writtenRep "Football"@en ;
  lexinfo:number   lexinfo:singular .

:Football-1411_plural a   lemon:Form ;
  lemon:writtenRep "Football"@en ;
  lexinfo:number    lexinfo:plural .

:Football-1411_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Football-1411_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Football-1411_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/coach> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Football-1411_sense_1_condition .

:Football-1411_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/HockeyTeam> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :Football-1411_form_preposition .
## Prepositions ##
:Football-1411_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

