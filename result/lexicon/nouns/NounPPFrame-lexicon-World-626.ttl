@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :World-626 ;
  lemon:entry    :World-626_form_preposition .

:World-626 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :World-626_form ;
  lemon:otherForm  :World-626_singular ;
  lemon:otherForm  :World-626_plural ;
  lemon:sense          :World-626_sense_1 ;
  lemon:synBehavior    :World-626_nounpp .

:World-626_form a lemon:Form ;
  lemon:writtenRep "World"@en .

:World-626_singular a    lemon:Form ;
  lemon:writtenRep "World"@en ;
  lexinfo:number   lexinfo:singular .

:World-626_plural a   lemon:Form ;
  lemon:writtenRep "World"@en ;
  lexinfo:number    lexinfo:plural .

:World-626_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:World-626_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :World-626_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/battle> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :World-626_sense_1_condition .

:World-626_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/MilitaryPerson> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/MilitaryConflict> .

:arg2 lemon:marker :World-626_form_preposition .
## Prepositions ##
:World-626_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

