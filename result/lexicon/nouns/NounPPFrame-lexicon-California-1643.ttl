@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :California-1643 ;
  lemon:entry    :California-1643_form_preposition .

:California-1643 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :California-1643_form ;
  lemon:otherForm  :California-1643_singular ;
  lemon:otherForm  :California-1643_plural ;
  lemon:sense          :California-1643_sense_1 ;
  lemon:synBehavior    :California-1643_nounpp .

:California-1643_form a lemon:Form ;
  lemon:writtenRep "California"@en .

:California-1643_singular a    lemon:Form ;
  lemon:writtenRep "California"@en ;
  lexinfo:number   lexinfo:singular .

:California-1643_plural a   lemon:Form ;
  lemon:writtenRep "California"@en ;
  lexinfo:number    lexinfo:plural .

:California-1643_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:California-1643_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :California-1643_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/country> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :California-1643_sense_1_condition .

:California-1643_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :California-1643_form_preposition .
## Prepositions ##
:California-1643_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

