@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :hockey-792 ;
  lemon:entry    :hockey-792_form_preposition .

:hockey-792 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :hockey-792_form ;
  lemon:otherForm  :hockey-792_singular ;
  lemon:otherForm  :hockey-792_plural ;
  lemon:sense          :hockey-792_sense_1 ;
  lemon:synBehavior    :hockey-792_nounpp .

:hockey-792_form a lemon:Form ;
  lemon:writtenRep "hockey"@en .

:hockey-792_singular a    lemon:Form ;
  lemon:writtenRep "hockey"@en ;
  lexinfo:number   lexinfo:singular .

:hockey-792_plural a   lemon:Form ;
  lemon:writtenRep "hockey"@en ;
  lexinfo:number    lexinfo:plural .

:hockey-792_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:hockey-792_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :hockey-792_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/birthPlace> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :hockey-792_sense_1_condition .

:hockey-792_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/resource/Surfing> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :hockey-792_form_preposition .
## Prepositions ##
:hockey-792_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

