@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :city-6022 ;
  lemon:entry    :city-6022_form_preposition .

:city-6022 a       lemon:LexicalEntry ;
