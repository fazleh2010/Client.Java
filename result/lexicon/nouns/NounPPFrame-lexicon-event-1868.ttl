@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :event-1868 ;
  lemon:entry    :event-1868_form_preposition .

:event-1868 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :event-1868_form ;
  lemon:otherForm  :event-1868_singular ;
  lemon:otherForm  :event-1868_plural ;
  lemon:sense          :event-1868_sense_1 ;
  lemon:synBehavior    :event-1868_nounpp .

:event-1868_form a lemon:Form ;
  lemon:writtenRep "event"@en .

:event-1868_singular a    lemon:Form ;
  lemon:writtenRep "event"@en ;
  lexinfo:number   lexinfo:singular .

:event-1868_plural a   lemon:Form ;
  lemon:writtenRep "event"@en ;
  lexinfo:number    lexinfo:plural .

:event-1868_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:event-1868_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :event-1868_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/date> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :event-1868_sense_1_condition .

:event-1868_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Event> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :event-1868_form_preposition .
## Prepositions ##
:event-1868_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

