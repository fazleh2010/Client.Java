@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :wines-8117 ;
  lemon:entry    :wines-8117_form_preposition .

:wines-8117 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :wines-8117_form ;
  lemon:otherForm  :wines-8117_singular ;
  lemon:otherForm  :wines-8117_plural ;
  lemon:sense          :wines-8117_sense_1 ;
  lemon:synBehavior    :wines-8117_nounpp .

:wines-8117_form a lemon:Form ;
  lemon:writtenRep "wines"@en .

:wines-8117_singular a    lemon:Form ;
  lemon:writtenRep "wines"@en ;
  lexinfo:number   lexinfo:singular .

:wines-8117_plural a   lemon:Form ;
  lemon:writtenRep "wines"@en ;
  lexinfo:number    lexinfo:plural .

:wines-8117_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:wines-8117_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :wines-8117_sense_1 ;
