@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Bank-6217 ;
  lemon:entry    :Bank-6217_form_preposition .

:Bank-6217 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Bank-6217_form ;
  lemon:otherForm  :Bank-6217_singular ;
  lemon:otherForm  :Bank-6217_plural ;
  lemon:sense          :Bank-6217_sense_1 ;
  lemon:synBehavior    :Bank-6217_nounpp .

:Bank-6217_form a lemon:Form ;
  lemon:writtenRep "Bank"@en .

:Bank-6217_singular a    lemon:Form ;
  lemon:writtenRep "Bank"@en ;
  lexinfo:number   lexinfo:singular .

:Bank-6217_plural a   lemon:Form ;
  lemon:writtenRep "Bank"@en ;
  lexinfo:number    lexinfo:plural .

:Bank-6217_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Bank-6217_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Bank-6217_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/product> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Bank-6217_sense_1_condition .

:Bank-6217_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Ship> .

:arg2 lemon:marker :Bank-6217_form_preposition .
## Prepositions ##
:Bank-6217_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

