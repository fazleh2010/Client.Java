@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :cricket-40311 ;
  lemon:entry    :cricket-40311_form_preposition .

:cricket-40311 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :cricket-40311_form ;
  lemon:otherForm  :cricket-40311_singular ;
  lemon:otherForm  :cricket-40311_plural ;
  lemon:sense          :cricket-40311_sense_1 ;
  lemon:synBehavior    :cricket-40311_nounpp .

:cricket-40311_form a lemon:Form ;
  lemon:writtenRep "cricket"@en .

:cricket-40311_singular a    lemon:Form ;
  lemon:writtenRep "cricket"@en ;
  lexinfo:number   lexinfo:singular .

:cricket-40311_plural a   lemon:Form ;
  lemon:writtenRep "cricket"@en ;
  lexinfo:number    lexinfo:plural .

:cricket-40311_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:cricket-40311_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :cricket-40311_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/location> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :cricket-40311_sense_1_condition .

:cricket-40311_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Location> .

:arg2 lemon:marker :cricket-40311_form_preposition .
## Prepositions ##
:cricket-40311_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

