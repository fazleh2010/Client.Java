@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :magazine-2482 ;
  lemon:entry    :magazine-2482_form_preposition .

:magazine-2482 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :magazine-2482_form ;
  lemon:otherForm  :magazine-2482_singular ;
  lemon:otherForm  :magazine-2482_plural ;
  lemon:sense          :magazine-2482_sense_1 ;
  lemon:synBehavior    :magazine-2482_nounpp .

:magazine-2482_form a lemon:Form ;
  lemon:writtenRep "magazine"@en .

:magazine-2482_singular a    lemon:Form ;
  lemon:writtenRep "magazine"@en ;
  lexinfo:number   lexinfo:singular .

:magazine-2482_plural a   lemon:Form ;
  lemon:writtenRep "magazine"@en ;
  lexinfo:number    lexinfo:plural .

:magazine-2482_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:magazine-2482_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :magazine-2482_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/editor> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :magazine-2482_sense_1_condition .

:magazine-2482_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :magazine-2482_form_preposition .
## Prepositions ##
:magazine-2482_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

