@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :curler-1883 ;
  lemon:entry    :curler-1883_form_preposition .

:curler-1883 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :curler-1883_form ;
  lemon:otherForm  :curler-1883_singular ;
  lemon:otherForm  :curler-1883_plural ;
  lemon:sense          :curler-1883_sense_1 ;
  lemon:synBehavior    :curler-1883_nounpp .

:curler-1883_form a lemon:Form ;
  lemon:writtenRep "curler"@en .

:curler-1883_singular a    lemon:Form ;
  lemon:writtenRep "curler"@en ;
  lexinfo:number   lexinfo:singular .

:curler-1883_plural a   lemon:Form ;
  lemon:writtenRep "curler"@en ;
  lexinfo:number    lexinfo:plural .

:curler-1883_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:curler-1883_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :curler-1883_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/deathDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :curler-1883_sense_1_condition .

:curler-1883_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :curler-1883_form_preposition .
## Prepositions ##
:curler-1883_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

