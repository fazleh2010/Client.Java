@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :team-45595 ;
  lemon:entry    :team-45595_form_preposition .

:team-45595 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :team-45595_form ;
  lemon:otherForm  :team-45595_singular ;
  lemon:otherForm  :team-45595_plural ;
  lemon:sense          :team-45595_sense_1 ;
  lemon:synBehavior    :team-45595_nounpp .

:team-45595_form a lemon:Form ;
  lemon:writtenRep "team"@en .

:team-45595_singular a    lemon:Form ;
  lemon:writtenRep "team"@en ;
  lexinfo:number   lexinfo:singular .

:team-45595_plural a   lemon:Form ;
  lemon:writtenRep "team"@en ;
  lexinfo:number    lexinfo:plural .

:team-45595_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:team-45595_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :team-45595_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/nickname> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :team-45595_sense_1_condition .

:team-45595_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#String> .

:arg2 lemon:marker :team-45595_form_preposition .
## Prepositions ##
:team-45595_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

