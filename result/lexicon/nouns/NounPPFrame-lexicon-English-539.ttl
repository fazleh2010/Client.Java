@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :English-539 ;
  lemon:entry    :English-539_form_preposition .

:English-539 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :English-539_form ;
  lemon:otherForm  :English-539_singular ;
  lemon:otherForm  :English-539_plural ;
  lemon:sense          :English-539_sense_1 ;
  lemon:synBehavior    :English-539_nounpp .

:English-539_form a lemon:Form ;
  lemon:writtenRep "English"@en .

:English-539_singular a    lemon:Form ;
  lemon:writtenRep "English"@en ;
  lexinfo:number   lexinfo:singular .

:English-539_plural a   lemon:Form ;
  lemon:writtenRep "English"@en ;
  lexinfo:number    lexinfo:plural .

:English-539_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:English-539_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :English-539_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/associatedMusicalArtist> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :English-539_sense_1_condition .

:English-539_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Band> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Place> .

:arg2 lemon:marker :English-539_form_preposition .
## Prepositions ##
:English-539_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

