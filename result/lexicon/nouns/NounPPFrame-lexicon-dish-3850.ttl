@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :dish-3850 ;
  lemon:entry    :dish-3850_form_preposition .

:dish-3850 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :dish-3850_form ;
  lemon:otherForm  :dish-3850_singular ;
  lemon:otherForm  :dish-3850_plural ;
  lemon:sense          :dish-3850_sense_1 ;
  lemon:synBehavior    :dish-3850_nounpp .

:dish-3850_form a lemon:Form ;
  lemon:writtenRep "dish"@en .

:dish-3850_singular a    lemon:Form ;
  lemon:writtenRep "dish"@en ;
  lexinfo:number   lexinfo:singular .

:dish-3850_plural a   lemon:Form ;
  lemon:writtenRep "dish"@en ;
  lexinfo:number    lexinfo:plural .

:dish-3850_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:dish-3850_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :dish-3850_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/ingredient> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :dish-3850_sense_1_condition .

:dish-3850_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Food> ;
  lemon:propertyRange   <http://www.w3.org/2002/07/owl#Thing> .

:arg2 lemon:marker :dish-3850_form_preposition .
## Prepositions ##
:dish-3850_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

