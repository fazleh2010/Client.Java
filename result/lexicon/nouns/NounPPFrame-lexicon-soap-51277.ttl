@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :soap-51277 ;
  lemon:entry    :soap-51277_form_preposition .

:soap-51277 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :soap-51277_form ;
  lemon:otherForm  :soap-51277_singular ;
  lemon:otherForm  :soap-51277_plural ;
  lemon:sense          :soap-51277_sense_1 ;
  lemon:synBehavior    :soap-51277_nounpp .

:soap-51277_form a lemon:Form ;
  lemon:writtenRep "soap"@en .

:soap-51277_singular a    lemon:Form ;
  lemon:writtenRep "soap"@en ;
  lexinfo:number   lexinfo:singular .

:soap-51277_plural a   lemon:Form ;
  lemon:writtenRep "soap"@en ;
  lexinfo:number    lexinfo:plural .

:soap-51277_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:soap-51277_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :soap-51277_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/portrayer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :soap-51277_sense_1_condition .

:soap-51277_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/FictionalCharacter> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :soap-51277_form_preposition .
## Prepositions ##
:soap-51277_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

