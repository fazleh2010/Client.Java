@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :actress-824 ;
  lemon:entry    :actress-824_form_preposition .

:actress-824 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :actress-824_form ;
  lemon:otherForm  :actress-824_singular ;
  lemon:otherForm  :actress-824_plural ;
  lemon:sense          :actress-824_sense_1 ;
  lemon:synBehavior    :actress-824_nounpp .

:actress-824_form a lemon:Form ;
  lemon:writtenRep "actress"@en .

:actress-824_singular a    lemon:Form ;
  lemon:writtenRep "actress"@en ;
  lexinfo:number   lexinfo:singular .

:actress-824_plural a   lemon:Form ;
  lemon:writtenRep "actress"@en ;
  lexinfo:number    lexinfo:plural .

:actress-824_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:actress-824_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :actress-824_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/birthYear> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :actress-824_sense_1_condition .

:actress-824_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :actress-824_form_preposition .
## Prepositions ##
:actress-824_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

