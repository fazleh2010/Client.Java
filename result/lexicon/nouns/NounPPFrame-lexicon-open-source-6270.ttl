@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :open-source-6270 ;
  lemon:entry    :open-source-6270_form_preposition .

:open-source-6270 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :open-source-6270_form ;
  lemon:otherForm  :open-source-6270_singular ;
  lemon:otherForm  :open-source-6270_plural ;
  lemon:sense          :open-source-6270_sense_1 ;
  lemon:synBehavior    :open-source-6270_nounpp .

:open-source-6270_form a lemon:Form ;
  lemon:writtenRep "open-source"@en .

:open-source-6270_singular a    lemon:Form ;
  lemon:writtenRep "open-source"@en ;
  lexinfo:number   lexinfo:singular .

:open-source-6270_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:open-source-6270_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:open-source-6270_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :open-source-6270_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/programmingLanguage> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :open-source-6270_sense_1_condition .

:open-source-6270_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Language> .

:arg2 lemon:marker :open-source-6270_form_preposition .
## Prepositions ##
:open-source-6270_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

