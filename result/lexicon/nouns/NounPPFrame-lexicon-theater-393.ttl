@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :theater-393 ;
  lemon:entry    :theater-393_form_preposition .

:theater-393 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :theater-393_form ;
  lemon:otherForm  :theater-393_singular ;
  lemon:otherForm  :theater-393_plural ;
  lemon:sense          :theater-393_sense_1 ;
  lemon:synBehavior    :theater-393_nounpp .

:theater-393_form a lemon:Form ;
  lemon:writtenRep "theater"@en .

:theater-393_singular a    lemon:Form ;
  lemon:writtenRep "theater"@en ;
  lexinfo:number   lexinfo:singular .

:theater-393_plural a   lemon:Form ;
  lemon:writtenRep "theater"@en ;
  lexinfo:number    lexinfo:plural .

:theater-393_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:theater-393_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :theater-393_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/architect> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :theater-393_sense_1_condition .

:theater-393_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Architect> .

:arg2 lemon:marker :theater-393_form_preposition .
## Prepositions ##
:theater-393_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

