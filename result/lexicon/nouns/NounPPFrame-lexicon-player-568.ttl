@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :player-568 ;
  lemon:entry    :player-568_form_preposition .

:player-568 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :player-568_form ;
  lemon:otherForm  :player-568_singular ;
  lemon:otherForm  :player-568_plural ;
  lemon:sense          :player-568_sense_1 ;
  lemon:synBehavior    :player-568_nounpp .

:player-568_form a lemon:Form ;
  lemon:writtenRep "player"@en .

:player-568_singular a    lemon:Form ;
  lemon:writtenRep "player"@en ;
  lexinfo:number   lexinfo:singular .

:player-568_plural a   lemon:Form ;
  lemon:writtenRep "player"@en ;
  lexinfo:number    lexinfo:plural .

:player-568_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:player-568_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :player-568_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/award> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :player-568_sense_1_condition .

:player-568_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Scientist> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Award> .

:arg2 lemon:marker :player-568_form_preposition .
## Prepositions ##
:player-568_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

