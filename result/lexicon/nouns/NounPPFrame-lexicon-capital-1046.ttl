@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :capital-1046 ;
  lemon:entry    :capital-1046_form_preposition .

:capital-1046 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :capital-1046_form ;
  lemon:otherForm  :capital-1046_singular ;
  lemon:otherForm  :capital-1046_plural ;
  lemon:sense          :capital-1046_sense_1 ;
  lemon:synBehavior    :capital-1046_nounpp .

:capital-1046_form a lemon:Form ;
  lemon:writtenRep "capital"@en .

:capital-1046_singular a    lemon:Form ;
  lemon:writtenRep "capital"@en ;
  lexinfo:number   lexinfo:singular .

:capital-1046_plural a   lemon:Form ;
  lemon:writtenRep "capital"@en ;
  lexinfo:number    lexinfo:plural .

:capital-1046_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:capital-1046_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :capital-1046_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/capital> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :capital-1046_sense_1_condition .

:capital-1046_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :capital-1046_form_preposition .
## Prepositions ##
:capital-1046_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

