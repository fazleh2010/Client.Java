@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :population-6030 ;
  lemon:entry    :population-6030_form_preposition .

:population-6030 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :population-6030_form ;
  lemon:otherForm  :population-6030_singular ;
  lemon:otherForm  :population-6030_plural ;
  lemon:sense          :population-6030_sense_1 ;
  lemon:synBehavior    :population-6030_nounpp .

:population-6030_form a lemon:Form ;
  lemon:writtenRep "population"@en .

:population-6030_singular a    lemon:Form ;
  lemon:writtenRep "population"@en ;
  lexinfo:number   lexinfo:singular .

:population-6030_plural a   lemon:Form ;
  lemon:writtenRep "population"@en ;
  lexinfo:number    lexinfo:plural .

:population-6030_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:population-6030_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :population-6030_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/populationTotal> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :population-6030_sense_1_condition .

:population-6030_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :population-6030_form_preposition .
## Prepositions ##
:population-6030_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

