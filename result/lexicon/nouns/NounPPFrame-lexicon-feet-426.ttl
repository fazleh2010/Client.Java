@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :feet-426 ;
  lemon:entry    :feet-426_form_preposition .

:feet-426 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :feet-426_form ;
  lemon:otherForm  :feet-426_singular ;
  lemon:otherForm  :feet-426_plural ;
  lemon:sense          :feet-426_sense_1 ;
  lemon:synBehavior    :feet-426_nounpp .

:feet-426_form a lemon:Form ;
  lemon:writtenRep "feet"@en .

:feet-426_singular a    lemon:Form ;
  lemon:writtenRep "feet"@en ;
  lexinfo:number   lexinfo:singular .

:feet-426_plural a   lemon:Form ;
  lemon:writtenRep "feet"@en ;
  lexinfo:number    lexinfo:plural .

:feet-426_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:feet-426_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :feet-426_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/areaCode> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :feet-426_sense_1_condition .

:feet-426_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :feet-426_form_preposition .
## Prepositions ##
:feet-426_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

