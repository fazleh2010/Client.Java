@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :rower-1644 ;
  lemon:entry    :rower-1644_form_preposition .

:rower-1644 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :rower-1644_form ;
  lemon:otherForm  :rower-1644_singular ;
  lemon:otherForm  :rower-1644_plural ;
  lemon:sense          :rower-1644_sense_1 ;
  lemon:synBehavior    :rower-1644_nounpp .

:rower-1644_form a lemon:Form ;
  lemon:writtenRep "rower"@en .

:rower-1644_singular a    lemon:Form ;
  lemon:writtenRep "rower"@en ;
  lexinfo:number   lexinfo:singular .

:rower-1644_plural a   lemon:Form ;
  lemon:writtenRep "rower"@en ;
  lexinfo:number    lexinfo:plural .

:rower-1644_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:rower-1644_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :rower-1644_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/country> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :rower-1644_sense_1_condition .

:rower-1644_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :rower-1644_form_preposition .
## Prepositions ##
:rower-1644_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

