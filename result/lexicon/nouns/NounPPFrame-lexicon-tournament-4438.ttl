@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :tournament-4438 ;
  lemon:entry    :tournament-4438_form_preposition .

:tournament-4438 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :tournament-4438_form ;
  lemon:otherForm  :tournament-4438_singular ;
  lemon:otherForm  :tournament-4438_plural ;
  lemon:sense          :tournament-4438_sense_1 ;
  lemon:synBehavior    :tournament-4438_nounpp .

:tournament-4438_form a lemon:Form ;
  lemon:writtenRep "tournament"@en .

:tournament-4438_singular a    lemon:Form ;
  lemon:writtenRep "tournament"@en ;
  lexinfo:number   lexinfo:singular .

:tournament-4438_plural a   lemon:Form ;
  lemon:writtenRep "tournament"@en ;
  lexinfo:number    lexinfo:plural .

:tournament-4438_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:tournament-4438_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :tournament-4438_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/location> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :tournament-4438_sense_1_condition .

:tournament-4438_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Building> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :tournament-4438_form_preposition .
## Prepositions ##
:tournament-4438_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

