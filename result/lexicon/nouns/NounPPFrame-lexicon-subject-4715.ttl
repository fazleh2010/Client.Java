@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :subject-4715 ;
  lemon:entry    :subject-4715_form_preposition .

:subject-4715 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :subject-4715_form ;
  lemon:otherForm  :subject-4715_singular ;
  lemon:otherForm  :subject-4715_plural ;
  lemon:sense          :subject-4715_sense_1 ;
  lemon:synBehavior    :subject-4715_nounpp .

:subject-4715_form a lemon:Form ;
  lemon:writtenRep "subject"@en .

:subject-4715_singular a    lemon:Form ;
  lemon:writtenRep "subject"@en ;
  lexinfo:number   lexinfo:singular .

:subject-4715_plural a   lemon:Form ;
  lemon:writtenRep "subject"@en ;
  lexinfo:number    lexinfo:plural .

:subject-4715_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:subject-4715_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :subject-4715_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/leaderName> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :subject-4715_sense_1_condition .

:subject-4715_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :subject-4715_form_preposition .
## Prepositions ##
:subject-4715_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

