@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lies-2279 ;
  lemon:entry    :lies-2279_form_preposition .

:lies-2279 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lies-2279_form ;
  lemon:otherForm  :lies-2279_singular ;
  lemon:otherForm  :lies-2279_plural ;
  lemon:sense          :lies-2279_sense_1 ;
  lemon:synBehavior    :lies-2279_nounpp .

:lies-2279_form a lemon:Form ;
  lemon:writtenRep "lies"@en .

:lies-2279_singular a    lemon:Form ;
  lemon:writtenRep "lies"@en ;
  lexinfo:number   lexinfo:singular .

:lies-2279_plural a   lemon:Form ;
  lemon:writtenRep "lies"@en ;
  lexinfo:number    lexinfo:plural .

:lies-2279_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:lies-2279_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lies-2279_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/diameter> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :lies-2279_sense_1_condition .

:lies-2279_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Device> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :lies-2279_form_preposition .
## Prepositions ##
:lies-2279_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

