@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Pope-7475 ;
  lemon:entry    :Pope-7475_form_preposition .

:Pope-7475 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Pope-7475_form ;
  lemon:otherForm  :Pope-7475_singular ;
  lemon:otherForm  :Pope-7475_plural ;
  lemon:sense          :Pope-7475_sense_1 ;
  lemon:synBehavior    :Pope-7475_nounpp .

:Pope-7475_form a lemon:Form ;
  lemon:writtenRep "Pope"@en .

:Pope-7475_singular a    lemon:Form ;
  lemon:writtenRep "Pope"@en ;
  lexinfo:number   lexinfo:singular .

:Pope-7475_plural a   lemon:Form ;
  lemon:writtenRep "Pope"@en ;
  lexinfo:number    lexinfo:plural .

:Pope-7475_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Pope-7475_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Pope-7475_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/successor> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Pope-7475_sense_1_condition .

:Pope-7475_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Politician> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :Pope-7475_form_preposition .
## Prepositions ##
:Pope-7475_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

