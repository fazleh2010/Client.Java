@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :variety-8111 ;
  lemon:entry    :variety-8111_form_preposition .

:variety-8111 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :variety-8111_form ;
  lemon:otherForm  :variety-8111_singular ;
  lemon:otherForm  :variety-8111_plural ;
  lemon:sense          :variety-8111_sense_1 ;
  lemon:synBehavior    :variety-8111_nounpp .

:variety-8111_form a lemon:Form ;
  lemon:writtenRep "variety"@en .

:variety-8111_singular a    lemon:Form ;
  lemon:writtenRep "variety"@en ;
  lexinfo:number   lexinfo:singular .

:variety-8111_plural a   lemon:Form ;
  lemon:writtenRep "variety"@en ;
  lexinfo:number    lexinfo:plural .

:variety-8111_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:variety-8111_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :variety-8111_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/wineRegion> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :variety-8111_sense_1_condition .

:variety-8111_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Grape> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Place> .

:arg2 lemon:marker :variety-8111_form_preposition .
## Prepositions ##
:variety-8111_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

