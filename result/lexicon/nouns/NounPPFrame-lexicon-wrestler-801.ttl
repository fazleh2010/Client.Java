@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :wrestler-801 ;
  lemon:entry    :wrestler-801_form_preposition .

:wrestler-801 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :wrestler-801_form ;
  lemon:otherForm  :wrestler-801_singular ;
  lemon:otherForm  :wrestler-801_plural ;
  lemon:sense          :wrestler-801_sense_1 ;
  lemon:synBehavior    :wrestler-801_nounpp .

:wrestler-801_form a lemon:Form ;
  lemon:writtenRep "wrestler"@en .

:wrestler-801_singular a    lemon:Form ;
  lemon:writtenRep "wrestler"@en ;
  lexinfo:number   lexinfo:singular .

:wrestler-801_plural a   lemon:Form ;
  lemon:writtenRep "wrestler"@en ;
  lexinfo:number    lexinfo:plural .

:wrestler-801_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:wrestler-801_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :wrestler-801_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/birthPlace> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :wrestler-801_sense_1_condition .

:wrestler-801_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/resource/Surfing> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :wrestler-801_form_preposition .
## Prepositions ##
:wrestler-801_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

