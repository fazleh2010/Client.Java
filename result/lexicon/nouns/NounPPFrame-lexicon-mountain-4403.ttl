@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :mountain-4403 ;
  lemon:entry    :mountain-4403_form_preposition .

:mountain-4403 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :mountain-4403_form ;
  lemon:otherForm  :mountain-4403_singular ;
  lemon:otherForm  :mountain-4403_plural ;
  lemon:sense          :mountain-4403_sense_1 ;
  lemon:synBehavior    :mountain-4403_nounpp .

:mountain-4403_form a lemon:Form ;
  lemon:writtenRep "mountain"@en .

:mountain-4403_singular a    lemon:Form ;
  lemon:writtenRep "mountain"@en ;
  lexinfo:number   lexinfo:singular .

:mountain-4403_plural a   lemon:Form ;
  lemon:writtenRep "mountain"@en ;
  lexinfo:number    lexinfo:plural .

:mountain-4403_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:mountain-4403_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :mountain-4403_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/locatedInArea> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :mountain-4403_sense_1_condition .

:mountain-4403_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :mountain-4403_form_preposition .
## Prepositions ##
:mountain-4403_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

