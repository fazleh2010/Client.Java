@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bank-6216 ;
  lemon:entry    :bank-6216_form_preposition .

:bank-6216 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :bank-6216_form ;
  lemon:otherForm  :bank-6216_singular ;
  lemon:otherForm  :bank-6216_plural ;
  lemon:sense          :bank-6216_sense_1 ;
  lemon:synBehavior    :bank-6216_nounpp .

:bank-6216_form a lemon:Form ;
  lemon:writtenRep "bank"@en .

:bank-6216_singular a    lemon:Form ;
  lemon:writtenRep "bank"@en ;
  lexinfo:number   lexinfo:singular .

:bank-6216_plural a   lemon:Form ;
  lemon:writtenRep "bank"@en ;
  lexinfo:number    lexinfo:plural .

:bank-6216_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:bank-6216_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :bank-6216_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/product> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :bank-6216_sense_1_condition .

:bank-6216_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Ship> .

:arg2 lemon:marker :bank-6216_form_preposition .
## Prepositions ##
:bank-6216_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

