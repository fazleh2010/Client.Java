@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :States-6600 ;
  lemon:entry    :States-6600_form_preposition .

:States-6600 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :States-6600_form ;
  lemon:otherForm  :States-6600_singular ;
  lemon:otherForm  :States-6600_plural ;
  lemon:sense          :States-6600_sense_1 ;
  lemon:synBehavior    :States-6600_nounpp .

:States-6600_form a lemon:Form ;
  lemon:writtenRep "States"@en .

:States-6600_singular a    lemon:Form ;
  lemon:writtenRep "States"@en ;
  lexinfo:number   lexinfo:singular .

:States-6600_plural a   lemon:Form ;
  lemon:writtenRep "States"@en ;
  lexinfo:number    lexinfo:plural .

:States-6600_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:States-6600_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :States-6600_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/restingPlace> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :States-6600_sense_1_condition .

:States-6600_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :States-6600_form_preposition .
## Prepositions ##
:States-6600_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

