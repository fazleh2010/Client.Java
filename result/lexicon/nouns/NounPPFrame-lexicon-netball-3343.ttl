@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :netball-3343 ;
  lemon:entry    :netball-3343_form_preposition .

:netball-3343 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :netball-3343_form ;
  lemon:otherForm  :netball-3343_singular ;
  lemon:otherForm  :netball-3343_plural ;
  lemon:sense          :netball-3343_sense_1 ;
  lemon:synBehavior    :netball-3343_nounpp .

:netball-3343_form a lemon:Form ;
  lemon:writtenRep "netball"@en .

:netball-3343_singular a    lemon:Form ;
  lemon:writtenRep "netball"@en ;
  lexinfo:number   lexinfo:singular .

:netball-3343_plural a   lemon:Form ;
  lemon:writtenRep "netball"@en ;
  lexinfo:number    lexinfo:plural .

:netball-3343_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:netball-3343_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :netball-3343_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/height> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :netball-3343_sense_1_condition .

:netball-3343_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/BasketballPlayer> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :netball-3343_form_preposition .
## Prepositions ##
:netball-3343_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

