@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :show-1535 ;
  lemon:entry    :show-1535_form_preposition .

:show-1535 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :show-1535_form ;
  lemon:otherForm  :show-1535_singular ;
  lemon:otherForm  :show-1535_plural ;
  lemon:sense          :show-1535_sense_1 ;
  lemon:synBehavior    :show-1535_nounpp .

:show-1535_form a lemon:Form ;
  lemon:writtenRep "show"@en .

:show-1535_singular a    lemon:Form ;
  lemon:writtenRep "show"@en ;
  lexinfo:number   lexinfo:singular .

:show-1535_plural a   lemon:Form ;
  lemon:writtenRep "show"@en ;
  lexinfo:number    lexinfo:plural .

:show-1535_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:show-1535_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :show-1535_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/completionDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :show-1535_sense_1_condition .

:show-1535_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Ship> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :show-1535_form_preposition .
## Prepositions ##
:show-1535_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

