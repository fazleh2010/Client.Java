@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :painter-4888 ;
  lemon:entry    :painter-4888_form_preposition .

:painter-4888 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :painter-4888_form ;
  lemon:otherForm  :painter-4888_singular ;
  lemon:otherForm  :painter-4888_plural ;
  lemon:sense          :painter-4888_sense_1 ;
  lemon:synBehavior    :painter-4888_nounpp .

:painter-4888_form a lemon:Form ;
  lemon:writtenRep "painter"@en .

:painter-4888_singular a    lemon:Form ;
  lemon:writtenRep "painter"@en ;
  lexinfo:number   lexinfo:singular .

:painter-4888_plural a   lemon:Form ;
  lemon:writtenRep "painter"@en ;
  lexinfo:number    lexinfo:plural .

:painter-4888_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:painter-4888_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :painter-4888_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/movement> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :painter-4888_sense_1_condition .

:painter-4888_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Artist> ;
  lemon:propertyRange   <http://www.w3.org/2002/07/owl#Thing> .

:arg2 lemon:marker :painter-4888_form_preposition .
## Prepositions ##
:painter-4888_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

