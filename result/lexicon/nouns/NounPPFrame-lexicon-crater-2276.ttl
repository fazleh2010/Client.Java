@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :crater-2276 ;
  lemon:entry    :crater-2276_form_preposition .

:crater-2276 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :crater-2276_form ;
  lemon:otherForm  :crater-2276_singular ;
  lemon:otherForm  :crater-2276_plural ;
  lemon:sense          :crater-2276_sense_1 ;
  lemon:synBehavior    :crater-2276_nounpp .

:crater-2276_form a lemon:Form ;
  lemon:writtenRep "crater"@en .

:crater-2276_singular a    lemon:Form ;
  lemon:writtenRep "crater"@en ;
  lexinfo:number   lexinfo:singular .

:crater-2276_plural a   lemon:Form ;
  lemon:writtenRep "crater"@en ;
  lexinfo:number    lexinfo:plural .

:crater-2276_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:crater-2276_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :crater-2276_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/diameter> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :crater-2276_sense_1_condition .

:crater-2276_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Device> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :crater-2276_form_preposition .
## Prepositions ##
:crater-2276_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

