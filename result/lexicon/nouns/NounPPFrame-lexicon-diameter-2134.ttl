@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :diameter-2134 ;
  lemon:entry    :diameter-2134_form_preposition .

:diameter-2134 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :diameter-2134_form ;
  lemon:otherForm  :diameter-2134_singular ;
  lemon:otherForm  :diameter-2134_plural ;
  lemon:sense          :diameter-2134_sense_1 ;
  lemon:synBehavior    :diameter-2134_nounpp .

:diameter-2134_form a lemon:Form ;
  lemon:writtenRep "diameter"@en .

:diameter-2134_singular a    lemon:Form ;
  lemon:writtenRep "diameter"@en ;
  lexinfo:number   lexinfo:singular .

:diameter-2134_plural a   lemon:Form ;
  lemon:writtenRep "diameter"@en ;
  lexinfo:number    lexinfo:plural .

:diameter-2134_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:diameter-2134_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :diameter-2134_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/discoverer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :diameter-2134_sense_1_condition .

:diameter-2134_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Planet> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :diameter-2134_form_preposition .
## Prepositions ##
:diameter-2134_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

