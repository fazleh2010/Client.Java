@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Pope-1894 ;
  lemon:entry    :Pope-1894_form_preposition .

:Pope-1894 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Pope-1894_form ;
  lemon:otherForm  :Pope-1894_singular ;
  lemon:otherForm  :Pope-1894_plural ;
  lemon:sense          :Pope-1894_sense_1 ;
  lemon:synBehavior    :Pope-1894_nounpp .

:Pope-1894_form a lemon:Form ;
  lemon:writtenRep "Pope"@en .

:Pope-1894_singular a    lemon:Form ;
  lemon:writtenRep "Pope"@en ;
  lexinfo:number   lexinfo:singular .

:Pope-1894_plural a   lemon:Form ;
  lemon:writtenRep "Pope"@en ;
  lexinfo:number    lexinfo:plural .

:Pope-1894_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Pope-1894_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Pope-1894_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/deathDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Pope-1894_sense_1_condition .

:Pope-1894_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :Pope-1894_form_preposition .
## Prepositions ##
:Pope-1894_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

