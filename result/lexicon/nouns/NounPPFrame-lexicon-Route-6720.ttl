@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Route-6720 ;
  lemon:entry    :Route-6720_form_preposition .

:Route-6720 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Route-6720_form ;
  lemon:otherForm  :Route-6720_singular ;
  lemon:otherForm  :Route-6720_plural ;
  lemon:sense          :Route-6720_sense_1 ;
  lemon:synBehavior    :Route-6720_nounpp .

:Route-6720_form a lemon:Form ;
  lemon:writtenRep "Route"@en .

:Route-6720_singular a    lemon:Form ;
  lemon:writtenRep "Route"@en ;
  lexinfo:number   lexinfo:singular .

:Route-6720_plural a   lemon:Form ;
  lemon:writtenRep "Route"@en ;
  lexinfo:number    lexinfo:plural .

:Route-6720_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Route-6720_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Route-6720_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/routeEnd> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Route-6720_sense_1_condition .

:Route-6720_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Road> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :Route-6720_form_preposition .
## Prepositions ##
:Route-6720_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

