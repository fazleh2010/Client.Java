@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :basketball-567 ;
  lemon:entry    :basketball-567_form_preposition .

:basketball-567 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :basketball-567_form ;
  lemon:otherForm  :basketball-567_singular ;
  lemon:otherForm  :basketball-567_plural ;
  lemon:sense          :basketball-567_sense_1 ;
  lemon:synBehavior    :basketball-567_nounpp .

:basketball-567_form a lemon:Form ;
  lemon:writtenRep "basketball"@en .

:basketball-567_singular a    lemon:Form ;
  lemon:writtenRep "basketball"@en ;
  lexinfo:number   lexinfo:singular .

:basketball-567_plural a   lemon:Form ;
  lemon:writtenRep "basketball"@en ;
  lexinfo:number    lexinfo:plural .

:basketball-567_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:basketball-567_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :basketball-567_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/award> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :basketball-567_sense_1_condition .

:basketball-567_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Scientist> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Award> .

:arg2 lemon:marker :basketball-567_form_preposition .
## Prepositions ##
:basketball-567_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

