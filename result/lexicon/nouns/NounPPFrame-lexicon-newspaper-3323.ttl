@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :newspaper-3323 ;
  lemon:entry    :newspaper-3323_form_preposition .

:newspaper-3323 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :newspaper-3323_form ;
  lemon:otherForm  :newspaper-3323_singular ;
  lemon:otherForm  :newspaper-3323_plural ;
  lemon:sense          :newspaper-3323_sense_1 ;
  lemon:synBehavior    :newspaper-3323_nounpp .

:newspaper-3323_form a lemon:Form ;
  lemon:writtenRep "newspaper"@en .

:newspaper-3323_singular a    lemon:Form ;
  lemon:writtenRep "newspaper"@en ;
  lexinfo:number   lexinfo:singular .

:newspaper-3323_plural a   lemon:Form ;
  lemon:writtenRep "newspaper"@en ;
  lexinfo:number    lexinfo:plural .

:newspaper-3323_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:newspaper-3323_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :newspaper-3323_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/headquarter> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :newspaper-3323_sense_1_condition .

:newspaper-3323_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/PopulatedPlace> .

:arg2 lemon:marker :newspaper-3323_form_preposition .
## Prepositions ##
:newspaper-3323_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

