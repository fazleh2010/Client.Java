@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :award-6137 ;
  lemon:entry    :award-6137_form_preposition .

:award-6137 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :award-6137_form ;
  lemon:otherForm  :award-6137_singular ;
  lemon:otherForm  :award-6137_plural ;
  lemon:sense          :award-6137_sense_1 ;
