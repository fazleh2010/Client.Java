@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :constituent-3962 ;
  lemon:entry    :constituent-3962_form_preposition .

:constituent-3962 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :constituent-3962_form ;
  lemon:otherForm  :constituent-3962_singular ;
  lemon:otherForm  :constituent-3962_plural ;
  lemon:sense          :constituent-3962_sense_1 ;
  lemon:synBehavior    :constituent-3962_nounpp .

:constituent-3962_form a lemon:Form ;
  lemon:writtenRep "constituent"@en .

:constituent-3962_singular a    lemon:Form ;
  lemon:writtenRep "constituent"@en ;
  lexinfo:number   lexinfo:singular .

:constituent-3962_plural a   lemon:Form ;
  lemon:writtenRep "constituent"@en ;
  lexinfo:number    lexinfo:plural .

:constituent-3962_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:constituent-3962_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :constituent-3962_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/isPartOf> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :constituent-3962_sense_1_condition .

:constituent-3962_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/AdministrativeRegion> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :constituent-3962_form_preposition .
## Prepositions ##
:constituent-3962_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

