@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Radio-6145 ;
  lemon:entry    :Radio-6145_form_preposition .

:Radio-6145 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Radio-6145_form ;
  lemon:otherForm  :Radio-6145_singular ;
  lemon:otherForm  :Radio-6145_plural ;
  lemon:sense          :Radio-6145_sense_1 ;
  lemon:synBehavior    :Radio-6145_nounpp .

:Radio-6145_form a lemon:Form ;
  lemon:writtenRep "Radio"@en .

:Radio-6145_singular a    lemon:Form ;
  lemon:writtenRep "Radio"@en ;
  lexinfo:number   lexinfo:singular .

:Radio-6145_plural a   lemon:Form ;
  lemon:writtenRep "Radio"@en ;
  lexinfo:number    lexinfo:plural .

:Radio-6145_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Radio-6145_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Radio-6145_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/presenter> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Radio-6145_sense_1_condition .

:Radio-6145_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :Radio-6145_form_preposition .
## Prepositions ##
:Radio-6145_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

