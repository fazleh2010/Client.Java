@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Kingdom-1044 ;
  lemon:entry    :Kingdom-1044_form_preposition .

:Kingdom-1044 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Kingdom-1044_form ;
  lemon:otherForm  :Kingdom-1044_singular ;
  lemon:otherForm  :Kingdom-1044_plural ;
  lemon:sense          :Kingdom-1044_sense_1 ;
  lemon:synBehavior    :Kingdom-1044_nounpp .

:Kingdom-1044_form a lemon:Form ;
  lemon:writtenRep "Kingdom"@en .

:Kingdom-1044_singular a    lemon:Form ;
  lemon:writtenRep "Kingdom"@en ;
  lexinfo:number   lexinfo:singular .

:Kingdom-1044_plural a   lemon:Form ;
  lemon:writtenRep "Kingdom"@en ;
  lexinfo:number    lexinfo:plural .

:Kingdom-1044_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Kingdom-1044_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Kingdom-1044_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/capital> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Kingdom-1044_sense_1_condition .

:Kingdom-1044_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :Kingdom-1044_form_preposition .
## Prepositions ##
:Kingdom-1044_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

