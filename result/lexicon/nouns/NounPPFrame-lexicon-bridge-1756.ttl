@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bridge-1756 ;
  lemon:entry    :bridge-1756_form_preposition .

:bridge-1756 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :bridge-1756_form ;
  lemon:otherForm  :bridge-1756_singular ;
  lemon:otherForm  :bridge-1756_plural ;
  lemon:sense          :bridge-1756_sense_1 ;
  lemon:synBehavior    :bridge-1756_nounpp .

:bridge-1756_form a lemon:Form ;
  lemon:writtenRep "bridge"@en .

:bridge-1756_singular a    lemon:Form ;
  lemon:writtenRep "bridge"@en ;
  lexinfo:number   lexinfo:singular .

:bridge-1756_plural a   lemon:Form ;
  lemon:writtenRep "bridge"@en ;
  lexinfo:number    lexinfo:plural .

:bridge-1756_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:bridge-1756_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :bridge-1756_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/crosses> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :bridge-1756_sense_1_condition .

:bridge-1756_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Bridge> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/River> .

:arg2 lemon:marker :bridge-1756_form_preposition .
## Prepositions ##
:bridge-1756_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

