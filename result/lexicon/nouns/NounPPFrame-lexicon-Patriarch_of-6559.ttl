@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Patriarch_of-6559 ;
  lemon:entry    :Patriarch_of-6559_form_preposition .

:Patriarch_of-6559 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Patriarch_of-6559_form ;
  lemon:otherForm  :Patriarch_of-6559_singular ;
  lemon:otherForm  :Patriarch_of-6559_plural ;
  lemon:sense          :Patriarch_of-6559_sense_1 ;
  lemon:synBehavior    :Patriarch_of-6559_nounpp .

:Patriarch_of-6559_form a lemon:Form ;
  lemon:writtenRep "Patriarch of"@en .

:Patriarch_of-6559_singular a    lemon:Form ;
  lemon:writtenRep "Patriarch of"@en ;
  lexinfo:number   lexinfo:singular .

:Patriarch_of-6559_plural a   lemon:Form ;
  lemon:writtenRep "Patriarch of"@en ;
  lexinfo:number    lexinfo:plural .

:Patriarch_of-6559_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Patriarch_of-6559_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Patriarch_of-6559_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/religion> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Patriarch_of-6559_sense_1_condition .

:Patriarch_of-6559_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :Patriarch_of-6559_form_preposition .
## Prepositions ##
:Patriarch_of-6559_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

