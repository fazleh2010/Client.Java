@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Roman-6555 ;
  lemon:entry    :Roman-6555_form_preposition .

:Roman-6555 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Roman-6555_form ;
  lemon:otherForm  :Roman-6555_singular ;
  lemon:otherForm  :Roman-6555_plural ;
  lemon:sense          :Roman-6555_sense_1 ;
  lemon:synBehavior    :Roman-6555_nounpp .

:Roman-6555_form a lemon:Form ;
  lemon:writtenRep "Roman"@en .

:Roman-6555_singular a    lemon:Form ;
  lemon:writtenRep "Roman"@en ;
  lexinfo:number   lexinfo:singular .

:Roman-6555_plural a   lemon:Form ;
  lemon:writtenRep "Roman"@en ;
  lexinfo:number    lexinfo:plural .

:Roman-6555_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Roman-6555_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Roman-6555_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/religion> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Roman-6555_sense_1_condition .

:Roman-6555_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :Roman-6555_form_preposition .
## Prepositions ##
:Roman-6555_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

