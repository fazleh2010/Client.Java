@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :county-4121 ;
  lemon:entry    :county-4121_form_preposition .

:county-4121 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :county-4121_form ;
  lemon:otherForm  :county-4121_singular ;
  lemon:otherForm  :county-4121_plural ;
  lemon:sense          :county-4121_sense_1 ;
  lemon:synBehavior    :county-4121_nounpp .

:county-4121_form a lemon:Form ;
  lemon:writtenRep "county"@en .

:county-4121_singular a    lemon:Form ;
  lemon:writtenRep "county"@en ;
  lexinfo:number   lexinfo:singular .

:county-4121_plural a   lemon:Form ;
  lemon:writtenRep "county"@en ;
  lexinfo:number    lexinfo:plural .

:county-4121_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:county-4121_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :county-4121_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/largestCity> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :county-4121_sense_1_condition .

:county-4121_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :county-4121_form_preposition .
## Prepositions ##
:county-4121_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

