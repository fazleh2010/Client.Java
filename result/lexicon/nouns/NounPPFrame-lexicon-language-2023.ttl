@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :language-2023 ;
  lemon:entry    :language-2023_form_preposition .

:language-2023 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :language-2023_form ;
  lemon:otherForm  :language-2023_singular ;
  lemon:otherForm  :language-2023_plural ;
  lemon:sense          :language-2023_sense_1 ;
  lemon:synBehavior    :language-2023_nounpp .

:language-2023_form a lemon:Form ;
  lemon:writtenRep "language"@en .

:language-2023_singular a    lemon:Form ;
  lemon:writtenRep "language"@en ;
  lexinfo:number   lexinfo:singular .

:language-2023_plural a   lemon:Form ;
  lemon:writtenRep "language"@en ;
  lexinfo:number    lexinfo:plural .

:language-2023_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:language-2023_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :language-2023_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/designer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :language-2023_sense_1_condition .

:language-2023_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Building> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :language-2023_form_preposition .
## Prepositions ##
:language-2023_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

