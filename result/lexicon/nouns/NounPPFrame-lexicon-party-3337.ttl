@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :party-3337 ;
  lemon:entry    :party-3337_form_preposition .

:party-3337 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :party-3337_form ;
  lemon:otherForm  :party-3337_singular ;
  lemon:otherForm  :party-3337_plural ;
  lemon:sense          :party-3337_sense_1 ;
  lemon:synBehavior    :party-3337_nounpp .

:party-3337_form a lemon:Form ;
  lemon:writtenRep "party"@en .

:party-3337_singular a    lemon:Form ;
  lemon:writtenRep "party"@en ;
  lexinfo:number   lexinfo:singular .

:party-3337_plural a   lemon:Form ;
  lemon:writtenRep "party"@en ;
  lexinfo:number    lexinfo:plural .

:party-3337_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:party-3337_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :party-3337_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/headquarter> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :party-3337_sense_1_condition .

:party-3337_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/PopulatedPlace> .

:arg2 lemon:marker :party-3337_form_preposition .
## Prepositions ##
:party-3337_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

