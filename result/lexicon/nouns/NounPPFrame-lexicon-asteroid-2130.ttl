@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :asteroid-2130 ;
  lemon:entry    :asteroid-2130_form_preposition .

:asteroid-2130 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :asteroid-2130_form ;
  lemon:otherForm  :asteroid-2130_singular ;
  lemon:otherForm  :asteroid-2130_plural ;
  lemon:sense          :asteroid-2130_sense_1 ;
  lemon:synBehavior    :asteroid-2130_nounpp .

:asteroid-2130_form a lemon:Form ;
  lemon:writtenRep "asteroid"@en .

:asteroid-2130_singular a    lemon:Form ;
  lemon:writtenRep "asteroid"@en ;
  lexinfo:number   lexinfo:singular .

:asteroid-2130_plural a   lemon:Form ;
  lemon:writtenRep "asteroid"@en ;
  lexinfo:number    lexinfo:plural .

:asteroid-2130_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:asteroid-2130_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :asteroid-2130_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/discoverer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :asteroid-2130_sense_1_condition .

:asteroid-2130_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Planet> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :asteroid-2130_form_preposition .
## Prepositions ##
:asteroid-2130_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

