@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :soap-6054 ;
  lemon:entry    :soap-6054_form_preposition .

:soap-6054 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :soap-6054_form ;
  lemon:otherForm  :soap-6054_singular ;
  lemon:otherForm  :soap-6054_plural ;
  lemon:sense          :soap-6054_sense_1 ;
  lemon:synBehavior    :soap-6054_nounpp .

:soap-6054_form a lemon:Form ;
  lemon:writtenRep "soap"@en .

:soap-6054_singular a    lemon:Form ;
  lemon:writtenRep "soap"@en ;
  lexinfo:number   lexinfo:singular .

:soap-6054_plural a   lemon:Form ;
  lemon:writtenRep "soap"@en ;
  lexinfo:number    lexinfo:plural .

:soap-6054_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:soap-6054_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :soap-6054_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/portrayer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :soap-6054_sense_1_condition .

:soap-6054_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Actor> .

:arg2 lemon:marker :soap-6054_form_preposition .
## Prepositions ##
:soap-6054_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

