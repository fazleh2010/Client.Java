@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :School-5287 ;
  lemon:entry    :School-5287_form_preposition .

:School-5287 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :School-5287_form ;
  lemon:otherForm  :School-5287_singular ;
  lemon:otherForm  :School-5287_plural ;
  lemon:sense          :School-5287_sense_1 ;
  lemon:synBehavior    :School-5287_nounpp .

:School-5287_form a lemon:Form ;
  lemon:writtenRep "School"@en .

:School-5287_singular a    lemon:Form ;
  lemon:writtenRep "School"@en ;
  lexinfo:number   lexinfo:singular .

:School-5287_plural a   lemon:Form ;
  lemon:writtenRep "School"@en ;
  lexinfo:number    lexinfo:plural .

:School-5287_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:School-5287_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :School-5287_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/numberOfStudents> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :School-5287_sense_1_condition .

:School-5287_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/School> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#nonNegativeInteger> .

:arg2 lemon:marker :School-5287_form_preposition .
## Prepositions ##
:School-5287_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

