@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Records-3031 ;
  lemon:entry    :Records-3031_form_preposition .

:Records-3031 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Records-3031_form ;
  lemon:otherForm  :Records-3031_singular ;
  lemon:otherForm  :Records-3031_plural ;
  lemon:sense          :Records-3031_sense_1 ;
  lemon:synBehavior    :Records-3031_nounpp .

:Records-3031_form a lemon:Form ;
  lemon:writtenRep "Records"@en .

:Records-3031_singular a    lemon:Form ;
  lemon:writtenRep "Records"@en ;
  lexinfo:number   lexinfo:singular .

:Records-3031_plural a   lemon:Form ;
  lemon:writtenRep "Records"@en ;
  lexinfo:number    lexinfo:plural .

:Records-3031_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Records-3031_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Records-3031_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/foundedBy> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Records-3031_sense_1_condition .

:Records-3031_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Agent> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Pope> .

:arg2 lemon:marker :Records-3031_form_preposition .
## Prepositions ##
:Records-3031_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

