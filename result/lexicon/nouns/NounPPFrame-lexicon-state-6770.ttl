@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :state-6770 ;
  lemon:entry    :state-6770_form_preposition .

:state-6770 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :state-6770_form ;
  lemon:otherForm  :state-6770_singular ;
  lemon:otherForm  :state-6770_plural ;
  lemon:sense          :state-6770_sense_1 ;
  lemon:synBehavior    :state-6770_nounpp .

:state-6770_form a lemon:Form ;
  lemon:writtenRep "state"@en .

:state-6770_singular a    lemon:Form ;
  lemon:writtenRep "state"@en ;
  lexinfo:number   lexinfo:singular .

:state-6770_plural a   lemon:Form ;
  lemon:writtenRep "state"@en ;
  lexinfo:number    lexinfo:plural .

:state-6770_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:state-6770_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :state-6770_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/routeStart> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :state-6770_sense_1_condition .

:state-6770_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Road> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Road> .

:arg2 lemon:marker :state-6770_form_preposition .
## Prepositions ##
:state-6770_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

