@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :golfer-4988 ;
  lemon:entry    :golfer-4988_form_preposition .

:golfer-4988 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :golfer-4988_form ;
  lemon:otherForm  :golfer-4988_singular ;
  lemon:otherForm  :golfer-4988_plural ;
  lemon:sense          :golfer-4988_sense_1 ;
  lemon:synBehavior    :golfer-4988_nounpp .

:golfer-4988_form a lemon:Form ;
  lemon:writtenRep "golfer"@en .

:golfer-4988_singular a    lemon:Form ;
  lemon:writtenRep "golfer"@en ;
  lexinfo:number   lexinfo:singular .

:golfer-4988_plural a   lemon:Form ;
  lemon:writtenRep "golfer"@en ;
  lexinfo:number    lexinfo:plural .

:golfer-4988_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:golfer-4988_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :golfer-4988_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/nationality> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :golfer-4988_sense_1_condition .

:golfer-4988_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :golfer-4988_form_preposition .
## Prepositions ##
:golfer-4988_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

