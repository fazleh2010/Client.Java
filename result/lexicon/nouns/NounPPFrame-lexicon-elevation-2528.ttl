@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :elevation-2528 ;
  lemon:entry    :elevation-2528_form_preposition .

:elevation-2528 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :elevation-2528_form ;
  lemon:otherForm  :elevation-2528_singular ;
  lemon:otherForm  :elevation-2528_plural ;
  lemon:sense          :elevation-2528_sense_1 ;
  lemon:synBehavior    :elevation-2528_nounpp .

:elevation-2528_form a lemon:Form ;
  lemon:writtenRep "elevation"@en .

:elevation-2528_singular a    lemon:Form ;
  lemon:writtenRep "elevation"@en ;
  lexinfo:number   lexinfo:singular .

:elevation-2528_plural a   lemon:Form ;
  lemon:writtenRep "elevation"@en ;
  lexinfo:number    lexinfo:plural .

:elevation-2528_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:elevation-2528_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :elevation-2528_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/elevation> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :elevation-2528_sense_1_condition .

:elevation-2528_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :elevation-2528_form_preposition .
## Prepositions ##
:elevation-2528_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

