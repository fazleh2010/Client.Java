@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Korea-5411 ;
  lemon:entry    :Korea-5411_form_preposition .

:Korea-5411 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Korea-5411_form ;
  lemon:otherForm  :Korea-5411_singular ;
  lemon:otherForm  :Korea-5411_plural ;
  lemon:sense          :Korea-5411_sense_1 ;
  lemon:synBehavior    :Korea-5411_nounpp .

:Korea-5411_form a lemon:Form ;
  lemon:writtenRep "Korea"@en .

:Korea-5411_singular a    lemon:Form ;
  lemon:writtenRep "Korea"@en ;
  lexinfo:number   lexinfo:singular .

:Korea-5411_plural a   lemon:Form ;
  lemon:writtenRep "Korea"@en ;
  lexinfo:number    lexinfo:plural .

:Korea-5411_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Korea-5411_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Korea-5411_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/openingDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Korea-5411_sense_1_condition .

:Korea-5411_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :Korea-5411_form_preposition .
## Prepositions ##
:Korea-5411_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

