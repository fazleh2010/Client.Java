@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :South-2021 ;
  lemon:entry    :South-2021_form_preposition .

:South-2021 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :South-2021_form ;
  lemon:otherForm  :South-2021_singular ;
  lemon:otherForm  :South-2021_plural ;
  lemon:sense          :South-2021_sense_1 ;
  lemon:synBehavior    :South-2021_nounpp .

:South-2021_form a lemon:Form ;
  lemon:writtenRep "South"@en .

:South-2021_singular a    lemon:Form ;
  lemon:writtenRep "South"@en ;
  lexinfo:number   lexinfo:singular .

:South-2021_plural a   lemon:Form ;
  lemon:writtenRep "South"@en ;
  lexinfo:number    lexinfo:plural .

:South-2021_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:South-2021_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :South-2021_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/designer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :South-2021_sense_1_condition .

:South-2021_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Building> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :South-2021_form_preposition .
## Prepositions ##
:South-2021_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

