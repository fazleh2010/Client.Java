@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :shopping-119 ;
  lemon:entry    :shopping-119_form_preposition .

:shopping-119 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :shopping-119_form ;
  lemon:otherForm  :shopping-119_singular ;
  lemon:otherForm  :shopping-119_plural ;
  lemon:sense          :shopping-119_sense_1 ;
  lemon:synBehavior    :shopping-119_nounpp .

:shopping-119_form a lemon:Form ;
  lemon:writtenRep "shopping"@en .

:shopping-119_singular a    lemon:Form ;
  lemon:writtenRep "shopping"@en ;
  lexinfo:number   lexinfo:singular .

:shopping-119_plural a   lemon:Form ;
  lemon:writtenRep "shopping"@en ;
  lexinfo:number    lexinfo:plural .

:shopping-119_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:shopping-119_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :shopping-119_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/address> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :shopping-119_sense_1_condition .

:shopping-119_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :shopping-119_form_preposition .
## Prepositions ##
:shopping-119_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

