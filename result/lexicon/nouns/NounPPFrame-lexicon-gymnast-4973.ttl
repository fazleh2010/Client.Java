@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :gymnast-4973 ;
  lemon:entry    :gymnast-4973_form_preposition .

:gymnast-4973 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :gymnast-4973_form ;
  lemon:otherForm  :gymnast-4973_singular ;
  lemon:otherForm  :gymnast-4973_plural ;
  lemon:sense          :gymnast-4973_sense_1 ;
  lemon:synBehavior    :gymnast-4973_nounpp .

:gymnast-4973_form a lemon:Form ;
  lemon:writtenRep "gymnast"@en .

:gymnast-4973_singular a    lemon:Form ;
  lemon:writtenRep "gymnast"@en ;
  lexinfo:number   lexinfo:singular .

:gymnast-4973_plural a   lemon:Form ;
  lemon:writtenRep "gymnast"@en ;
  lexinfo:number    lexinfo:plural .

:gymnast-4973_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:gymnast-4973_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :gymnast-4973_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/nationality> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :gymnast-4973_sense_1_condition .

:gymnast-4973_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :gymnast-4973_form_preposition .
## Prepositions ##
:gymnast-4973_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

