@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :feet-7670 ;
  lemon:entry    :feet-7670_form_preposition .

:feet-7670 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :feet-7670_form ;
  lemon:otherForm  :feet-7670_singular ;
  lemon:otherForm  :feet-7670_plural ;
  lemon:sense          :feet-7670_sense_1 ;
  lemon:synBehavior    :feet-7670_nounpp .

:feet-7670_form a lemon:Form ;
  lemon:writtenRep "feet"@en .

:feet-7670_singular a    lemon:Form ;
  lemon:writtenRep "feet"@en ;
  lexinfo:number   lexinfo:singular .

:feet-7670_plural a   lemon:Form ;
  lemon:writtenRep "feet"@en ;
  lexinfo:number    lexinfo:plural .

:feet-7670_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:feet-7670_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :feet-7670_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/timeZone> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :feet-7670_sense_1_condition .

:feet-7670_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :feet-7670_form_preposition .
## Prepositions ##
:feet-7670_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

