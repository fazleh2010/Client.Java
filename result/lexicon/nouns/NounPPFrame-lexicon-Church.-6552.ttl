@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Church.-6552 ;
  lemon:entry    :Church.-6552_form_preposition .

:Church.-6552 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Church.-6552_form ;
  lemon:otherForm  :Church.-6552_singular ;
  lemon:otherForm  :Church.-6552_plural ;
  lemon:sense          :Church.-6552_sense_1 ;
  lemon:synBehavior    :Church.-6552_nounpp .

:Church.-6552_form a lemon:Form ;
  lemon:writtenRep "Church."@en .

:Church.-6552_singular a    lemon:Form ;
  lemon:writtenRep "Church."@en ;
  lexinfo:number   lexinfo:singular .

:Church.-6552_plural a   lemon:Form ;
  lemon:writtenRep "Church."@en ;
  lexinfo:number    lexinfo:plural .

:Church.-6552_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Church.-6552_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Church.-6552_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/religion> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Church.-6552_sense_1_condition .

:Church.-6552_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :Church.-6552_form_preposition .
## Prepositions ##
:Church.-6552_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

