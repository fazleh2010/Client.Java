@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :darts-799 ;
  lemon:entry    :darts-799_form_preposition .

:darts-799 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :darts-799_form ;
  lemon:otherForm  :darts-799_singular ;
  lemon:otherForm  :darts-799_plural ;
  lemon:sense          :darts-799_sense_1 ;
  lemon:synBehavior    :darts-799_nounpp .

:darts-799_form a lemon:Form ;
  lemon:writtenRep "darts"@en .

:darts-799_singular a    lemon:Form ;
  lemon:writtenRep "darts"@en ;
  lexinfo:number   lexinfo:singular .

:darts-799_plural a   lemon:Form ;
  lemon:writtenRep "darts"@en ;
  lexinfo:number    lexinfo:plural .

:darts-799_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:darts-799_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :darts-799_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/birthPlace> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :darts-799_sense_1_condition .

:darts-799_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/resource/Surfing> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :darts-799_form_preposition .
## Prepositions ##
:darts-799_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

