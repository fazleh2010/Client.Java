@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Airport-7529 ;
  lemon:entry    :Airport-7529_form_preposition .

:Airport-7529 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Airport-7529_form ;
  lemon:otherForm  :Airport-7529_singular ;
  lemon:otherForm  :Airport-7529_plural ;
  lemon:sense          :Airport-7529_sense_1 ;
