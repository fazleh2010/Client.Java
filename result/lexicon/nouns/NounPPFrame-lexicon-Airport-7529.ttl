:Airport-7529_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Airport-7529_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
