@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :London-5448 ;
  lemon:entry    :London-5448_form_preposition .

:London-5448 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :London-5448_form ;
  lemon:otherForm  :London-5448_singular ;
  lemon:otherForm  :London-5448_plural ;
  lemon:sense          :London-5448_sense_1 ;
  lemon:synBehavior    :London-5448_nounpp .

:London-5448_form a lemon:Form ;
  lemon:writtenRep "London"@en .

:London-5448_singular a    lemon:Form ;
  lemon:writtenRep "London"@en ;
  lexinfo:number   lexinfo:singular .

:London-5448_plural a   lemon:Form ;
  lemon:writtenRep "London"@en ;
  lexinfo:number    lexinfo:plural .

:London-5448_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:London-5448_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :London-5448_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/operator> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :London-5448_sense_1_condition .

:London-5448_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/LaunchPad> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Organisation> .

:arg2 lemon:marker :London-5448_form_preposition .
## Prepositions ##
:London-5448_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

