@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Area-3280 ;
  lemon:entry    :Area-3280_form_preposition .

:Area-3280 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Area-3280_form ;
  lemon:otherForm  :Area-3280_singular ;
  lemon:otherForm  :Area-3280_plural ;
  lemon:sense          :Area-3280_sense_1 ;
  lemon:synBehavior    :Area-3280_nounpp .

:Area-3280_form a lemon:Form ;
  lemon:writtenRep "Area"@en .

:Area-3280_singular a    lemon:Form ;
  lemon:writtenRep "Area"@en ;
  lexinfo:number   lexinfo:singular .

:Area-3280_plural a   lemon:Form ;
  lemon:writtenRep "Area"@en ;
  lexinfo:number    lexinfo:plural .

:Area-3280_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Area-3280_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Area-3280_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/growingGrape> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Area-3280_sense_1_condition .

:Area-3280_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/WineRegion> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Grape> .

:arg2 lemon:marker :Area-3280_form_preposition .
## Prepositions ##
:Area-3280_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

