@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :City-392 ;
  lemon:entry    :City-392_form_preposition .

:City-392 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :City-392_form ;
  lemon:otherForm  :City-392_singular ;
  lemon:otherForm  :City-392_plural ;
  lemon:sense          :City-392_sense_1 ;
  lemon:synBehavior    :City-392_nounpp .

:City-392_form a lemon:Form ;
  lemon:writtenRep "City"@en .

:City-392_singular a    lemon:Form ;
  lemon:writtenRep "City"@en ;
  lexinfo:number   lexinfo:singular .

:City-392_plural a   lemon:Form ;
  lemon:writtenRep "City"@en ;
  lexinfo:number    lexinfo:plural .

:City-392_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:City-392_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :City-392_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/architect> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :City-392_sense_1_condition .

:City-392_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Architect> .

:arg2 lemon:marker :City-392_form_preposition .
## Prepositions ##
:City-392_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

