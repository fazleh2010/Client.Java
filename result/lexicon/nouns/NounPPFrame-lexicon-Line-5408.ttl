@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Line-5408 ;
  lemon:entry    :Line-5408_form_preposition .

:Line-5408 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Line-5408_form ;
  lemon:otherForm  :Line-5408_singular ;
  lemon:otherForm  :Line-5408_plural ;
  lemon:sense          :Line-5408_sense_1 ;
  lemon:synBehavior    :Line-5408_nounpp .

:Line-5408_form a lemon:Form ;
  lemon:writtenRep "Line"@en .

:Line-5408_singular a    lemon:Form ;
  lemon:writtenRep "Line"@en ;
  lexinfo:number   lexinfo:singular .

:Line-5408_plural a   lemon:Form ;
  lemon:writtenRep "Line"@en ;
  lexinfo:number    lexinfo:plural .

:Line-5408_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Line-5408_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Line-5408_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/openingDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Line-5408_sense_1_condition .

:Line-5408_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :Line-5408_form_preposition .
## Prepositions ##
:Line-5408_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

