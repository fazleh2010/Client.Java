@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :race-40838 ;
  lemon:entry    :race-40838_form_preposition .

:race-40838 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :race-40838_form ;
  lemon:otherForm  :race-40838_singular ;
  lemon:otherForm  :race-40838_plural ;
  lemon:sense          :race-40838_sense_1 ;
  lemon:synBehavior    :race-40838_nounpp .

:race-40838_form a lemon:Form ;
  lemon:writtenRep "race"@en .

:race-40838_singular a    lemon:Form ;
  lemon:writtenRep "race"@en ;
  lexinfo:number   lexinfo:singular .

:race-40838_plural a   lemon:Form ;
  lemon:writtenRep "race"@en ;
  lexinfo:number    lexinfo:plural .

:race-40838_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:race-40838_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :race-40838_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/location> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :race-40838_sense_1_condition .

:race-40838_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Location> .

:arg2 lemon:marker :race-40838_form_preposition .
## Prepositions ##
:race-40838_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

