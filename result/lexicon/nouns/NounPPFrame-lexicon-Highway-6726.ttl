@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Highway-6726 ;
  lemon:entry    :Highway-6726_form_preposition .

:Highway-6726 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Highway-6726_form ;
  lemon:otherForm  :Highway-6726_singular ;
  lemon:otherForm  :Highway-6726_plural ;
  lemon:sense          :Highway-6726_sense_1 ;
  lemon:synBehavior    :Highway-6726_nounpp .

:Highway-6726_form a lemon:Form ;
  lemon:writtenRep "Highway"@en .

:Highway-6726_singular a    lemon:Form ;
  lemon:writtenRep "Highway"@en ;
  lexinfo:number   lexinfo:singular .

:Highway-6726_plural a   lemon:Form ;
  lemon:writtenRep "Highway"@en ;
