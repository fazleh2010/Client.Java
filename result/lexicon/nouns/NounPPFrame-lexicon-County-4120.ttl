@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :County-4120 ;
  lemon:entry    :County-4120_form_preposition .

:County-4120 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :County-4120_form ;
  lemon:otherForm  :County-4120_singular ;
  lemon:otherForm  :County-4120_plural ;
  lemon:sense          :County-4120_sense_1 ;
  lemon:synBehavior    :County-4120_nounpp .

:County-4120_form a lemon:Form ;
  lemon:writtenRep "County"@en .

:County-4120_singular a    lemon:Form ;
  lemon:writtenRep "County"@en ;
  lexinfo:number   lexinfo:singular .

:County-4120_plural a   lemon:Form ;
  lemon:writtenRep "County"@en ;
  lexinfo:number    lexinfo:plural .

:County-4120_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:County-4120_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :County-4120_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/largestCity> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :County-4120_sense_1_condition .

:County-4120_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :County-4120_form_preposition .
## Prepositions ##
:County-4120_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

