@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Army-624 ;
  lemon:entry    :Army-624_form_preposition .

:Army-624 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Army-624_form ;
  lemon:otherForm  :Army-624_singular ;
  lemon:otherForm  :Army-624_plural ;
  lemon:sense          :Army-624_sense_1 ;
  lemon:synBehavior    :Army-624_nounpp .

:Army-624_form a lemon:Form ;
  lemon:writtenRep "Army"@en .

:Army-624_singular a    lemon:Form ;
  lemon:writtenRep "Army"@en ;
  lexinfo:number   lexinfo:singular .

:Army-624_plural a   lemon:Form ;
  lemon:writtenRep "Army"@en ;
  lexinfo:number    lexinfo:plural .

:Army-624_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Army-624_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Army-624_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/battle> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Army-624_sense_1_condition .

:Army-624_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/MilitaryPerson> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/MilitaryConflict> .

:arg2 lemon:marker :Army-624_form_preposition .
## Prepositions ##
:Army-624_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

