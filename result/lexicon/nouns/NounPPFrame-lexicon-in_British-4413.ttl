@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :in_British-4413 ;
  lemon:entry    :in_British-4413_form_preposition .

:in_British-4413 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :in_British-4413_form ;
  lemon:otherForm  :in_British-4413_singular ;
  lemon:otherForm  :in_British-4413_plural ;
  lemon:sense          :in_British-4413_sense_1 ;
  lemon:synBehavior    :in_British-4413_nounpp .

:in_British-4413_form a lemon:Form ;
  lemon:writtenRep "in British"@en .

:in_British-4413_singular a    lemon:Form ;
  lemon:writtenRep "in British"@en ;
  lexinfo:number   lexinfo:singular .

:in_British-4413_plural a   lemon:Form ;
  lemon:writtenRep "in British"@en ;
  lexinfo:number    lexinfo:plural .

:in_British-4413_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:in_British-4413_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :in_British-4413_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/locatedInArea> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :in_British-4413_sense_1_condition .

:in_British-4413_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :in_British-4413_form_preposition .
## Prepositions ##
:in_British-4413_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

