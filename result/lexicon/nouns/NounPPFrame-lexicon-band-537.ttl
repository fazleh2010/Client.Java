@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :band-537 ;
  lemon:entry    :band-537_form_preposition .

:band-537 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :band-537_form ;
  lemon:otherForm  :band-537_singular ;
  lemon:otherForm  :band-537_plural ;
  lemon:sense          :band-537_sense_1 ;
  lemon:synBehavior    :band-537_nounpp .

:band-537_form a lemon:Form ;
  lemon:writtenRep "band"@en .

:band-537_singular a    lemon:Form ;
  lemon:writtenRep "band"@en ;
  lexinfo:number   lexinfo:singular .

:band-537_plural a   lemon:Form ;
  lemon:writtenRep "band"@en ;
  lexinfo:number    lexinfo:plural .

:band-537_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:band-537_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :band-537_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/associatedMusicalArtist> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :band-537_sense_1_condition .

:band-537_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Band> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Place> .

:arg2 lemon:marker :band-537_form_preposition .
## Prepositions ##
:band-537_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

