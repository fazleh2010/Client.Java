@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Class-2017 ;
  lemon:entry    :Class-2017_form_preposition .

:Class-2017 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Class-2017_form ;
  lemon:otherForm  :Class-2017_singular ;
  lemon:otherForm  :Class-2017_plural ;
  lemon:sense          :Class-2017_sense_1 ;
  lemon:synBehavior    :Class-2017_nounpp .

:Class-2017_form a lemon:Form ;
  lemon:writtenRep "Class"@en .

:Class-2017_singular a    lemon:Form ;
  lemon:writtenRep "Class"@en ;
  lexinfo:number   lexinfo:singular .

:Class-2017_plural a   lemon:Form ;
  lemon:writtenRep "Class"@en ;
  lexinfo:number    lexinfo:plural .

:Class-2017_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Class-2017_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Class-2017_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/designer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Class-2017_sense_1_condition .

:Class-2017_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Building> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :Class-2017_form_preposition .
## Prepositions ##
:Class-2017_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

