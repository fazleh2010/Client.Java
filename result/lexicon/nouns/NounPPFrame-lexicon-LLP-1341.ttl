@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :LLP-1341 ;
  lemon:entry    :LLP-1341_form_preposition .

:LLP-1341 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :LLP-1341_form ;
  lemon:otherForm  :LLP-1341_singular ;
  lemon:otherForm  :LLP-1341_plural ;
  lemon:sense          :LLP-1341_sense_1 ;
  lemon:synBehavior    :LLP-1341_nounpp .

:LLP-1341_form a lemon:Form ;
  lemon:writtenRep "LLP"@en .

:LLP-1341_singular a    lemon:Form ;
  lemon:writtenRep "LLP"@en ;
  lexinfo:number   lexinfo:singular .

:LLP-1341_plural a   lemon:Form ;
  lemon:writtenRep "LLP"@en ;
  lexinfo:number    lexinfo:plural .

:LLP-1341_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:LLP-1341_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :LLP-1341_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/closingDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :LLP-1341_sense_1_condition .

:LLP-1341_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :LLP-1341_form_preposition .
## Prepositions ##
:LLP-1341_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

