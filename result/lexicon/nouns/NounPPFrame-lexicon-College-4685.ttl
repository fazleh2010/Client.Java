:College-4685_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:College-4685_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
