@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :College-4685 ;
  lemon:entry    :College-4685_form_preposition .

:College-4685 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :College-4685_form ;
  lemon:otherForm  :College-4685_singular ;
  lemon:otherForm  :College-4685_plural ;
  lemon:sense          :College-4685_sense_1 ;
  lemon:synBehavior    :College-4685_nounpp .

:College-4685_form a lemon:Form ;
