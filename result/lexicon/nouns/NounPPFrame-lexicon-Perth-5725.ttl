@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Perth-5725 ;
  lemon:entry    :Perth-5725_form_preposition .

:Perth-5725 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Perth-5725_form ;
  lemon:otherForm  :Perth-5725_singular ;
  lemon:otherForm  :Perth-5725_plural ;
  lemon:sense          :Perth-5725_sense_1 ;
  lemon:synBehavior    :Perth-5725_nounpp .

:Perth-5725_form a lemon:Form ;
  lemon:writtenRep "Perth"@en .

:Perth-5725_singular a    lemon:Form ;
  lemon:writtenRep "Perth"@en ;
  lexinfo:number   lexinfo:singular .

:Perth-5725_plural a   lemon:Form ;
  lemon:writtenRep "Perth"@en ;
  lexinfo:number    lexinfo:plural .

:Perth-5725_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Perth-5725_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Perth-5725_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/minimumTemperature> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Perth-5725_sense_1_condition .

:Perth-5725_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Planet> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :Perth-5725_form_preposition .
## Prepositions ##
:Perth-5725_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

