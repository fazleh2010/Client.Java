@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :offices-3335 ;
  lemon:entry    :offices-3335_form_preposition .

:offices-3335 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :offices-3335_form ;
  lemon:otherForm  :offices-3335_singular ;
  lemon:otherForm  :offices-3335_plural ;
  lemon:sense          :offices-3335_sense_1 ;
  lemon:synBehavior    :offices-3335_nounpp .

:offices-3335_form a lemon:Form ;
  lemon:writtenRep "offices"@en .

:offices-3335_singular a    lemon:Form ;
  lemon:writtenRep "offices"@en ;
  lexinfo:number   lexinfo:singular .

:offices-3335_plural a   lemon:Form ;
  lemon:writtenRep "offices"@en ;
  lexinfo:number    lexinfo:plural .

:offices-3335_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:offices-3335_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :offices-3335_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/headquarter> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :offices-3335_sense_1_condition .

:offices-3335_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/PopulatedPlace> .

:arg2 lemon:marker :offices-3335_form_preposition .
## Prepositions ##
:offices-3335_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

