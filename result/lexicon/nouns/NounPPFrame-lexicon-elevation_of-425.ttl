@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :elevation_of-425 ;
  lemon:entry    :elevation_of-425_form_preposition .

:elevation_of-425 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :elevation_of-425_form ;
  lemon:otherForm  :elevation_of-425_singular ;
  lemon:otherForm  :elevation_of-425_plural ;
  lemon:sense          :elevation_of-425_sense_1 ;
  lemon:synBehavior    :elevation_of-425_nounpp .

:elevation_of-425_form a lemon:Form ;
  lemon:writtenRep "elevation of"@en .

:elevation_of-425_singular a    lemon:Form ;
  lemon:writtenRep "elevation of"@en ;
  lexinfo:number   lexinfo:singular .

:elevation_of-425_plural a   lemon:Form ;
  lemon:writtenRep "elevation of"@en ;
  lexinfo:number    lexinfo:plural .

:elevation_of-425_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:elevation_of-425_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :elevation_of-425_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/areaCode> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :elevation_of-425_sense_1_condition .

:elevation_of-425_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :elevation_of-425_form_preposition .
## Prepositions ##
:elevation_of-425_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

