@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Assembly-4157 ;
  lemon:entry    :Assembly-4157_form_preposition .

:Assembly-4157 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Assembly-4157_form ;
  lemon:otherForm  :Assembly-4157_singular ;
  lemon:otherForm  :Assembly-4157_plural ;
  lemon:sense          :Assembly-4157_sense_1 ;
  lemon:synBehavior    :Assembly-4157_nounpp .

:Assembly-4157_form a lemon:Form ;
  lemon:writtenRep "Assembly"@en .

:Assembly-4157_singular a    lemon:Form ;
  lemon:writtenRep "Assembly"@en ;
  lexinfo:number   lexinfo:singular .

:Assembly-4157_plural a   lemon:Form ;
  lemon:writtenRep "Assembly"@en ;
  lexinfo:number    lexinfo:plural .

:Assembly-4157_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Assembly-4157_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Assembly-4157_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/lastElectionDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Assembly-4157_sense_1_condition .

:Assembly-4157_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :Assembly-4157_form_preposition .
## Prepositions ##
:Assembly-4157_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

