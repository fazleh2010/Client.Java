@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :story_by-551 ;
  lemon:entry    :story_by-551_form_preposition .

:story_by-551 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :story_by-551_form ;
  lemon:otherForm  :story_by-551_singular ;
  lemon:otherForm  :story_by-551_plural ;
  lemon:sense          :story_by-551_sense_1 ;
  lemon:synBehavior    :story_by-551_nounpp .

:story_by-551_form a lemon:Form ;
  lemon:writtenRep "story by"@en .

:story_by-551_singular a    lemon:Form ;
  lemon:writtenRep "story by"@en ;
  lexinfo:number   lexinfo:singular .

:story_by-551_plural a   lemon:Form ;
  lemon:writtenRep "story by"@en ;
  lexinfo:number    lexinfo:plural .

:story_by-551_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:story_by-551_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :story_by-551_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/author> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :story_by-551_sense_1_condition .

:story_by-551_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Book> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :story_by-551_form_preposition .
## Prepositions ##
:story_by-551_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

