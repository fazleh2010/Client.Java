@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :gold-1632 ;
  lemon:entry    :gold-1632_form_preposition .

:gold-1632 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :gold-1632_form ;
  lemon:otherForm  :gold-1632_singular ;
  lemon:otherForm  :gold-1632_plural ;
  lemon:sense          :gold-1632_sense_1 ;
  lemon:synBehavior    :gold-1632_nounpp .

:gold-1632_form a lemon:Form ;
  lemon:writtenRep "gold"@en .

:gold-1632_singular a    lemon:Form ;
  lemon:writtenRep "gold"@en ;
  lexinfo:number   lexinfo:singular .

:gold-1632_plural a   lemon:Form ;
  lemon:writtenRep "gold"@en ;
  lexinfo:number    lexinfo:plural .

:gold-1632_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:gold-1632_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :gold-1632_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/country> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :gold-1632_sense_1_condition .

:gold-1632_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :gold-1632_form_preposition .
## Prepositions ##
:gold-1632_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

