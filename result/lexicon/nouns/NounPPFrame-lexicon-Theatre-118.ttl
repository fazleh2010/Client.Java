@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Theatre-118 ;
  lemon:entry    :Theatre-118_form_preposition .

:Theatre-118 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Theatre-118_form ;
  lemon:otherForm  :Theatre-118_singular ;
  lemon:otherForm  :Theatre-118_plural ;
  lemon:sense          :Theatre-118_sense_1 ;
  lemon:synBehavior    :Theatre-118_nounpp .

:Theatre-118_form a lemon:Form ;
  lemon:writtenRep "Theatre"@en .

:Theatre-118_singular a    lemon:Form ;
  lemon:writtenRep "Theatre"@en ;
  lexinfo:number   lexinfo:singular .

:Theatre-118_plural a   lemon:Form ;
  lemon:writtenRep "Theatre"@en ;
  lexinfo:number    lexinfo:plural .

:Theatre-118_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Theatre-118_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Theatre-118_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/address> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Theatre-118_sense_1_condition .

:Theatre-118_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :Theatre-118_form_preposition .
## Prepositions ##
:Theatre-118_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

