@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :game-2035 ;
  lemon:entry    :game-2035_form_preposition .

:game-2035 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :game-2035_form ;
  lemon:otherForm  :game-2035_singular ;
  lemon:otherForm  :game-2035_plural ;
  lemon:sense          :game-2035_sense_1 ;
  lemon:synBehavior    :game-2035_nounpp .

:game-2035_form a lemon:Form ;
  lemon:writtenRep "game"@en .

:game-2035_singular a    lemon:Form ;
  lemon:writtenRep "game"@en ;
  lexinfo:number   lexinfo:singular .

:game-2035_plural a   lemon:Form ;
  lemon:writtenRep "game"@en ;
  lexinfo:number    lexinfo:plural .

:game-2035_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:game-2035_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :game-2035_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/developer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :game-2035_sense_1_condition .

:game-2035_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/VideoGame> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Agent> .

:arg2 lemon:marker :game-2035_form_preposition .
## Prepositions ##
:game-2035_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

