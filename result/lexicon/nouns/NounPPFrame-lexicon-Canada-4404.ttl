@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Canada-4404 ;
  lemon:entry    :Canada-4404_form_preposition .

:Canada-4404 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Canada-4404_form ;
  lemon:otherForm  :Canada-4404_singular ;
  lemon:otherForm  :Canada-4404_plural ;
  lemon:sense          :Canada-4404_sense_1 ;
  lemon:synBehavior    :Canada-4404_nounpp .

:Canada-4404_form a lemon:Form ;
  lemon:writtenRep "Canada"@en .

:Canada-4404_singular a    lemon:Form ;
  lemon:writtenRep "Canada"@en ;
  lexinfo:number   lexinfo:singular .

:Canada-4404_plural a   lemon:Form ;
  lemon:writtenRep "Canada"@en ;
  lexinfo:number    lexinfo:plural .

:Canada-4404_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Canada-4404_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Canada-4404_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/locatedInArea> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Canada-4404_sense_1_condition .

:Canada-4404_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :Canada-4404_form_preposition .
## Prepositions ##
:Canada-4404_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

