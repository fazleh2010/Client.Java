@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :pair-391 ;
  lemon:entry    :pair-391_form_preposition .

:pair-391 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :pair-391_form ;
  lemon:otherForm  :pair-391_singular ;
  lemon:otherForm  :pair-391_plural ;
  lemon:sense          :pair-391_sense_1 ;
  lemon:synBehavior    :pair-391_nounpp .

:pair-391_form a lemon:Form ;
  lemon:writtenRep "pair"@en .

:pair-391_singular a    lemon:Form ;
  lemon:writtenRep "pair"@en ;
  lexinfo:number   lexinfo:singular .

:pair-391_plural a   lemon:Form ;
  lemon:writtenRep "pair"@en ;
  lexinfo:number    lexinfo:plural .

:pair-391_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:pair-391_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :pair-391_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/architect> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :pair-391_sense_1_condition .

:pair-391_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Architect> .

:arg2 lemon:marker :pair-391_form_preposition .
## Prepositions ##
:pair-391_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

