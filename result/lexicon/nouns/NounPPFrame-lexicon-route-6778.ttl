@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :route-6778 ;
  lemon:entry    :route-6778_form_preposition .

:route-6778 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :route-6778_form ;
  lemon:otherForm  :route-6778_singular ;
  lemon:otherForm  :route-6778_plural ;
  lemon:sense          :route-6778_sense_1 ;
  lemon:synBehavior    :route-6778_nounpp .

:route-6778_form a lemon:Form ;
  lemon:writtenRep "route"@en .

:route-6778_singular a    lemon:Form ;
  lemon:writtenRep "route"@en ;
  lexinfo:number   lexinfo:singular .

:route-6778_plural a   lemon:Form ;
  lemon:writtenRep "route"@en ;
  lexinfo:number    lexinfo:plural .

:route-6778_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:route-6778_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :route-6778_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/routeStart> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :route-6778_sense_1_condition .

:route-6778_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Road> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Road> .

:arg2 lemon:marker :route-6778_form_preposition .
## Prepositions ##
:route-6778_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

