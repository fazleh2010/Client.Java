@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Special_Area-5729 ;
  lemon:entry    :Special_Area-5729_form_preposition .

:Special_Area-5729 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Special_Area-5729_form ;
  lemon:otherForm  :Special_Area-5729_singular ;
  lemon:otherForm  :Special_Area-5729_plural ;
  lemon:sense          :Special_Area-5729_sense_1 ;
  lemon:synBehavior    :Special_Area-5729_nounpp .

:Special_Area-5729_form a lemon:Form ;
  lemon:writtenRep "Special Area"@en .

:Special_Area-5729_singular a    lemon:Form ;
  lemon:writtenRep "Special Area"@en ;
  lexinfo:number   lexinfo:singular .

:Special_Area-5729_plural a   lemon:Form ;
  lemon:writtenRep "Special Area"@en ;
  lexinfo:number    lexinfo:plural .

:Special_Area-5729_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Special_Area-5729_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Special_Area-5729_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/minimumTemperature> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Special_Area-5729_sense_1_condition .

:Special_Area-5729_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Planet> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :Special_Area-5729_form_preposition .
## Prepositions ##
:Special_Area-5729_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

