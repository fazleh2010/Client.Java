@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :television-1532 ;
  lemon:entry    :television-1532_form_preposition .

:television-1532 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :television-1532_form ;
  lemon:otherForm  :television-1532_singular ;
  lemon:otherForm  :television-1532_plural ;
  lemon:sense          :television-1532_sense_1 ;
  lemon:synBehavior    :television-1532_nounpp .

:television-1532_form a lemon:Form ;
  lemon:writtenRep "television"@en .

:television-1532_singular a    lemon:Form ;
  lemon:writtenRep "television"@en ;
  lexinfo:number   lexinfo:singular .

:television-1532_plural a   lemon:Form ;
  lemon:writtenRep "television"@en ;
  lexinfo:number    lexinfo:plural .

:television-1532_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:television-1532_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :television-1532_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/completionDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :television-1532_sense_1_condition .

:television-1532_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Ship> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :television-1532_form_preposition .
## Prepositions ##
:television-1532_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

