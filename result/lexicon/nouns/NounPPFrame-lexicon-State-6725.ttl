@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :State-6725 ;
  lemon:entry    :State-6725_form_preposition .

:State-6725 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :State-6725_form ;
  lemon:otherForm  :State-6725_singular ;
  lemon:otherForm  :State-6725_plural ;
  lemon:sense          :State-6725_sense_1 ;
  lemon:synBehavior    :State-6725_nounpp .

:State-6725_form a lemon:Form ;
  lemon:writtenRep "State"@en .

:State-6725_singular a    lemon:Form ;
  lemon:writtenRep "State"@en ;
  lexinfo:number   lexinfo:singular .

:State-6725_plural a   lemon:Form ;
  lemon:writtenRep "State"@en ;
  lexinfo:number    lexinfo:plural .

:State-6725_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:State-6725_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :State-6725_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/routeEnd> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :State-6725_sense_1_condition .

:State-6725_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Road> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :State-6725_form_preposition .
## Prepositions ##
:State-6725_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

