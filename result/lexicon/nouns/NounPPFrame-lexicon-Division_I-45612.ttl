@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Division_I-45612 ;
  lemon:entry    :Division_I-45612_form_preposition .

:Division_I-45612 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Division_I-45612_form ;
  lemon:otherForm  :Division_I-45612_singular ;
  lemon:otherForm  :Division_I-45612_plural ;
  lemon:sense          :Division_I-45612_sense_1 ;
  lemon:synBehavior    :Division_I-45612_nounpp .

:Division_I-45612_form a lemon:Form ;
  lemon:writtenRep "Division I"@en .

:Division_I-45612_singular a    lemon:Form ;
  lemon:writtenRep "Division I"@en ;
  lexinfo:number   lexinfo:singular .

:Division_I-45612_plural a   lemon:Form ;
  lemon:writtenRep "Division I"@en ;
  lexinfo:number    lexinfo:plural .

:Division_I-45612_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Division_I-45612_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Division_I-45612_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/nickname> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Division_I-45612_sense_1_condition .

:Division_I-45612_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#String> .

:arg2 lemon:marker :Division_I-45612_form_preposition .
## Prepositions ##
:Division_I-45612_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

