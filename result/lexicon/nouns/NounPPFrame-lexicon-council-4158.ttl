@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :council-4158 ;
  lemon:entry    :council-4158_form_preposition .

:council-4158 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :council-4158_form ;
  lemon:otherForm  :council-4158_singular ;
  lemon:otherForm  :council-4158_plural ;
  lemon:sense          :council-4158_sense_1 ;
  lemon:synBehavior    :council-4158_nounpp .

:council-4158_form a lemon:Form ;
  lemon:writtenRep "council"@en .

:council-4158_singular a    lemon:Form ;
  lemon:writtenRep "council"@en ;
  lexinfo:number   lexinfo:singular .

:council-4158_plural a   lemon:Form ;
  lemon:writtenRep "council"@en ;
  lexinfo:number    lexinfo:plural .

:council-4158_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:council-4158_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :council-4158_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/lastElectionDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :council-4158_sense_1_condition .

:council-4158_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :council-4158_form_preposition .
## Prepositions ##
:council-4158_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

