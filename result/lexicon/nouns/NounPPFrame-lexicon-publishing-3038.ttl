@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :publishing-3038 ;
  lemon:entry    :publishing-3038_form_preposition .

:publishing-3038 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :publishing-3038_form ;
  lemon:otherForm  :publishing-3038_singular ;
  lemon:otherForm  :publishing-3038_plural ;
  lemon:sense          :publishing-3038_sense_1 ;
  lemon:synBehavior    :publishing-3038_nounpp .

:publishing-3038_form a lemon:Form ;
  lemon:writtenRep "publishing"@en .

:publishing-3038_singular a    lemon:Form ;
  lemon:writtenRep "publishing"@en ;
  lexinfo:number   lexinfo:singular .

:publishing-3038_plural a   lemon:Form ;
  lemon:writtenRep "publishing"@en ;
  lexinfo:number    lexinfo:plural .

:publishing-3038_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:publishing-3038_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :publishing-3038_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/founder> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :publishing-3038_sense_1_condition .

:publishing-3038_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Publisher> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Agent> .

:arg2 lemon:marker :publishing-3038_form_preposition .
## Prepositions ##
:publishing-3038_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

