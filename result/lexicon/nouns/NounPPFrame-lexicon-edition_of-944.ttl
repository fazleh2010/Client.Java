@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :edition_of-944 ;
  lemon:entry    :edition_of-944_form_preposition .

:edition_of-944 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :edition_of-944_form ;
  lemon:otherForm  :edition_of-944_singular ;
  lemon:otherForm  :edition_of-944_plural ;
  lemon:sense          :edition_of-944_sense_1 ;
  lemon:synBehavior    :edition_of-944_nounpp .

:edition_of-944_form a lemon:Form ;
  lemon:writtenRep "edition of"@en .

:edition_of-944_singular a    lemon:Form ;
  lemon:writtenRep "edition of"@en ;
  lexinfo:number   lexinfo:singular .

:edition_of-944_plural a   lemon:Form ;
  lemon:writtenRep "edition of"@en ;
  lexinfo:number    lexinfo:plural .

:edition_of-944_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:edition_of-944_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :edition_of-944_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/budget> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :edition_of-944_sense_1_condition .

:edition_of-944_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :edition_of-944_form_preposition .
## Prepositions ##
:edition_of-944_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

