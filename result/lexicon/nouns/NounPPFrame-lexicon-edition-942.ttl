@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :edition-942 ;
  lemon:entry    :edition-942_form_preposition .

:edition-942 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :edition-942_form ;
  lemon:otherForm  :edition-942_singular ;
  lemon:otherForm  :edition-942_plural ;
  lemon:sense          :edition-942_sense_1 ;
  lemon:synBehavior    :edition-942_nounpp .

:edition-942_form a lemon:Form ;
  lemon:writtenRep "edition"@en .

:edition-942_singular a    lemon:Form ;
  lemon:writtenRep "edition"@en ;
  lexinfo:number   lexinfo:singular .

:edition-942_plural a   lemon:Form ;
  lemon:writtenRep "edition"@en ;
  lexinfo:number    lexinfo:plural .

:edition-942_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:edition-942_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :edition-942_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/budget> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :edition-942_sense_1_condition .

:edition-942_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :edition-942_form_preposition .
## Prepositions ##
:edition-942_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

