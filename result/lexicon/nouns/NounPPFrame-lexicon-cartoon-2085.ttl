@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :cartoon-2085 ;
  lemon:entry    :cartoon-2085_form_preposition .

:cartoon-2085 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :cartoon-2085_form ;
  lemon:otherForm  :cartoon-2085_singular ;
  lemon:otherForm  :cartoon-2085_plural ;
  lemon:sense          :cartoon-2085_sense_1 ;
  lemon:synBehavior    :cartoon-2085_nounpp .

:cartoon-2085_form a lemon:Form ;
  lemon:writtenRep "cartoon"@en .

:cartoon-2085_singular a    lemon:Form ;
  lemon:writtenRep "cartoon"@en ;
  lexinfo:number   lexinfo:singular .

:cartoon-2085_plural a   lemon:Form ;
  lemon:writtenRep "cartoon"@en ;
  lexinfo:number    lexinfo:plural .

:cartoon-2085_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:cartoon-2085_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :cartoon-2085_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/director> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :cartoon-2085_sense_1_condition .

:cartoon-2085_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :cartoon-2085_form_preposition .
## Prepositions ##
:cartoon-2085_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

