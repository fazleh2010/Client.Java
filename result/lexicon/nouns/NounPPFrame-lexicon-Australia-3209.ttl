@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Australia-3209 ;
  lemon:entry    :Australia-3209_form_preposition .

:Australia-3209 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Australia-3209_form ;
  lemon:otherForm  :Australia-3209_singular ;
  lemon:otherForm  :Australia-3209_plural ;
  lemon:sense          :Australia-3209_sense_1 ;
  lemon:synBehavior    :Australia-3209_nounpp .

:Australia-3209_form a lemon:Form ;
  lemon:writtenRep "Australia"@en .

:Australia-3209_singular a    lemon:Form ;
  lemon:writtenRep "Australia"@en ;
  lexinfo:number   lexinfo:singular .

:Australia-3209_plural a   lemon:Form ;
  lemon:writtenRep "Australia"@en ;
  lexinfo:number    lexinfo:plural .

:Australia-3209_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Australia-3209_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Australia-3209_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/governor> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Australia-3209_sense_1_condition .

:Australia-3209_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :Australia-3209_form_preposition .
## Prepositions ##
:Australia-3209_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

