@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :highway-6718 ;
  lemon:entry    :highway-6718_form_preposition .

:highway-6718 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :highway-6718_form ;
  lemon:otherForm  :highway-6718_singular ;
  lemon:otherForm  :highway-6718_plural ;
  lemon:sense          :highway-6718_sense_1 ;
  lemon:synBehavior    :highway-6718_nounpp .

:highway-6718_form a lemon:Form ;
  lemon:writtenRep "highway"@en .

:highway-6718_singular a    lemon:Form ;
  lemon:writtenRep "highway"@en ;
  lexinfo:number   lexinfo:singular .

:highway-6718_plural a   lemon:Form ;
  lemon:writtenRep "highway"@en ;
  lexinfo:number    lexinfo:plural .

:highway-6718_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:highway-6718_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :highway-6718_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/routeEnd> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :highway-6718_sense_1_condition .

:highway-6718_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Road> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :highway-6718_form_preposition .
## Prepositions ##
:highway-6718_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

