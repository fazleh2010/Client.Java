@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :team-4290 ;
  lemon:entry    :team-4290_form_preposition .

:team-4290 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :team-4290_form ;
  lemon:otherForm  :team-4290_singular ;
  lemon:otherForm  :team-4290_plural ;
  lemon:sense          :team-4290_sense_1 ;
  lemon:synBehavior    :team-4290_nounpp .

:team-4290_form a lemon:Form ;
  lemon:writtenRep "team"@en .

:team-4290_singular a    lemon:Form ;
  lemon:writtenRep "team"@en ;
  lexinfo:number   lexinfo:singular .

:team-4290_plural a   lemon:Form ;
  lemon:writtenRep "team"@en ;
  lexinfo:number    lexinfo:plural .

:team-4290_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:team-4290_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :team-4290_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/league> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :team-4290_sense_1_condition .

:team-4290_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/HockeyTeam> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/IceHockeyLeague> .

:arg2 lemon:marker :team-4290_form_preposition .
## Prepositions ##
:team-4290_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

