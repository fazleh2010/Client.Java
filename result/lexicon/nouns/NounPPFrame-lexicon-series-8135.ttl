@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :series-8135 ;
  lemon:entry    :series-8135_form_preposition .

:series-8135 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :series-8135_form ;
  lemon:otherForm  :series-8135_singular ;
  lemon:otherForm  :series-8135_plural ;
  lemon:sense          :series-8135_sense_1 ;
  lemon:synBehavior    :series-8135_nounpp .

:series-8135_form a lemon:Form ;
  lemon:writtenRep "series"@en .

:series-8135_singular a    lemon:Form ;
  lemon:writtenRep "series"@en ;
  lexinfo:number   lexinfo:singular .

:series-8135_plural a   lemon:Form ;
  lemon:writtenRep "series"@en ;
  lexinfo:number    lexinfo:plural .

:series-8135_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:series-8135_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :series-8135_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/writer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :series-8135_sense_1_condition .

:series-8135_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Song> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :series-8135_form_preposition .
## Prepositions ##
:series-8135_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

