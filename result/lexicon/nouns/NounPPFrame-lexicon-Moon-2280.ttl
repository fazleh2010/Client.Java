@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Moon-2280 ;
  lemon:entry    :Moon-2280_form_preposition .

:Moon-2280 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Moon-2280_form ;
  lemon:otherForm  :Moon-2280_singular ;
  lemon:otherForm  :Moon-2280_plural ;
  lemon:sense          :Moon-2280_sense_1 ;
  lemon:synBehavior    :Moon-2280_nounpp .

:Moon-2280_form a lemon:Form ;
  lemon:writtenRep "Moon"@en .

:Moon-2280_singular a    lemon:Form ;
  lemon:writtenRep "Moon"@en ;
  lexinfo:number   lexinfo:singular .

:Moon-2280_plural a   lemon:Form ;
  lemon:writtenRep "Moon"@en ;
  lexinfo:number    lexinfo:plural .

:Moon-2280_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Moon-2280_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Moon-2280_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/diameter> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Moon-2280_sense_1_condition .

:Moon-2280_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Device> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :Moon-2280_form_preposition .
## Prepositions ##
:Moon-2280_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

