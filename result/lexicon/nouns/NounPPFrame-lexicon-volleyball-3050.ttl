@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :volleyball-3050 ;
  lemon:entry    :volleyball-3050_form_preposition .

:volleyball-3050 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :volleyball-3050_form ;
  lemon:otherForm  :volleyball-3050_singular ;
  lemon:otherForm  :volleyball-3050_plural ;
  lemon:sense          :volleyball-3050_sense_1 ;
  lemon:synBehavior    :volleyball-3050_nounpp .

:volleyball-3050_form a lemon:Form ;
  lemon:writtenRep "volleyball"@en .

:volleyball-3050_singular a    lemon:Form ;
  lemon:writtenRep "volleyball"@en ;
  lexinfo:number   lexinfo:singular .

:volleyball-3050_plural a   lemon:Form ;
  lemon:writtenRep "volleyball"@en ;
  lexinfo:number    lexinfo:plural .

:volleyball-3050_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:volleyball-3050_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :volleyball-3050_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/foundingYear> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :volleyball-3050_sense_1_condition .

:volleyball-3050_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#gYear> .

:arg2 lemon:marker :volleyball-3050_form_preposition .
## Prepositions ##
:volleyball-3050_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

