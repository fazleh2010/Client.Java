@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Alexandria-4984 ;
  lemon:entry    :Alexandria-4984_form_preposition .

:Alexandria-4984 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Alexandria-4984_form ;
  lemon:otherForm  :Alexandria-4984_singular ;
  lemon:otherForm  :Alexandria-4984_plural ;
  lemon:sense          :Alexandria-4984_sense_1 ;
  lemon:synBehavior    :Alexandria-4984_nounpp .

:Alexandria-4984_form a lemon:Form ;
  lemon:writtenRep "Alexandria"@en .

:Alexandria-4984_singular a    lemon:Form ;
  lemon:writtenRep "Alexandria"@en ;
  lexinfo:number   lexinfo:singular .

:Alexandria-4984_plural a   lemon:Form ;
  lemon:writtenRep "Alexandria"@en ;
  lexinfo:number    lexinfo:plural .

:Alexandria-4984_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Alexandria-4984_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Alexandria-4984_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/nationality> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Alexandria-4984_sense_1_condition .

:Alexandria-4984_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :Alexandria-4984_form_preposition .
## Prepositions ##
:Alexandria-4984_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

