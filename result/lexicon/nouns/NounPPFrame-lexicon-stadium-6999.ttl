@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :stadium-6999 ;
  lemon:entry    :stadium-6999_form_preposition .

:stadium-6999 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :stadium-6999_form ;
  lemon:otherForm  :stadium-6999_singular ;
  lemon:otherForm  :stadium-6999_plural ;
  lemon:sense          :stadium-6999_sense_1 ;
  lemon:synBehavior    :stadium-6999_nounpp .

:stadium-6999_form a lemon:Form ;
  lemon:writtenRep "stadium"@en .

:stadium-6999_singular a    lemon:Form ;
  lemon:writtenRep "stadium"@en ;
  lexinfo:number   lexinfo:singular .

:stadium-6999_plural a   lemon:Form ;
  lemon:writtenRep "stadium"@en ;
  lexinfo:number    lexinfo:plural .

:stadium-6999_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:stadium-6999_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :stadium-6999_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/seatingCapacity> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :stadium-6999_sense_1_condition .

:stadium-6999_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Stadium> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#nonNegativeInteger> .

:arg2 lemon:marker :stadium-6999_form_preposition .
## Prepositions ##
:stadium-6999_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

