@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :services-6220 ;
  lemon:entry    :services-6220_form_preposition .

:services-6220 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :services-6220_form ;
  lemon:otherForm  :services-6220_singular ;
  lemon:otherForm  :services-6220_plural ;
  lemon:sense          :services-6220_sense_1 ;
  lemon:synBehavior    :services-6220_nounpp .

:services-6220_form a lemon:Form ;
  lemon:writtenRep "services"@en .

:services-6220_singular a    lemon:Form ;
  lemon:writtenRep "services"@en ;
  lexinfo:number   lexinfo:singular .

:services-6220_plural a   lemon:Form ;
  lemon:writtenRep "services"@en ;
  lexinfo:number    lexinfo:plural .

:services-6220_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:services-6220_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :services-6220_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/product> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :services-6220_sense_1_condition .

:services-6220_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Ship> .

:arg2 lemon:marker :services-6220_form_preposition .
## Prepositions ##
:services-6220_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

