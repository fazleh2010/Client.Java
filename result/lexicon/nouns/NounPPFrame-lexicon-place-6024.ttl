@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :place-6024 ;
  lemon:entry    :place-6024_form_preposition .

:place-6024 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :place-6024_form ;
  lemon:otherForm  :place-6024_singular ;
  lemon:otherForm  :place-6024_plural ;
  lemon:sense          :place-6024_sense_1 ;
  lemon:synBehavior    :place-6024_nounpp .

:place-6024_form a lemon:Form ;
  lemon:writtenRep "place"@en .

:place-6024_singular a    lemon:Form ;
  lemon:writtenRep "place"@en ;
  lexinfo:number   lexinfo:singular .

:place-6024_plural a   lemon:Form ;
  lemon:writtenRep "place"@en ;
  lexinfo:number    lexinfo:plural .

:place-6024_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:place-6024_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :place-6024_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/populationTotal> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :place-6024_sense_1_condition .

:place-6024_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :place-6024_form_preposition .
## Prepositions ##
:place-6024_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

