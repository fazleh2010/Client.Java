@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :event-4434 ;
  lemon:entry    :event-4434_form_preposition .

:event-4434 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :event-4434_form ;
  lemon:otherForm  :event-4434_singular ;
  lemon:otherForm  :event-4434_plural ;
  lemon:sense          :event-4434_sense_1 ;
  lemon:synBehavior    :event-4434_nounpp .

:event-4434_form a lemon:Form ;
  lemon:writtenRep "event"@en .

:event-4434_singular a    lemon:Form ;
  lemon:writtenRep "event"@en ;
  lexinfo:number   lexinfo:singular .

:event-4434_plural a   lemon:Form ;
  lemon:writtenRep "event"@en ;
  lexinfo:number    lexinfo:plural .

:event-4434_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:event-4434_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :event-4434_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/location> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :event-4434_sense_1_condition .

:event-4434_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Building> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :event-4434_form_preposition .
## Prepositions ##
:event-4434_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

