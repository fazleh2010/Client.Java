@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :school-5278 ;
  lemon:entry    :school-5278_form_preposition .

:school-5278 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :school-5278_form ;
  lemon:otherForm  :school-5278_singular ;
  lemon:otherForm  :school-5278_plural ;
  lemon:sense          :school-5278_sense_1 ;
  lemon:synBehavior    :school-5278_nounpp .

:school-5278_form a lemon:Form ;
  lemon:writtenRep "school"@en .

:school-5278_singular a    lemon:Form ;
  lemon:writtenRep "school"@en ;
  lexinfo:number   lexinfo:singular .

:school-5278_plural a   lemon:Form ;
  lemon:writtenRep "school"@en ;
  lexinfo:number    lexinfo:plural .

:school-5278_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:school-5278_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :school-5278_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/numberOfStudents> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :school-5278_sense_1_condition .

:school-5278_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/School> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#nonNegativeInteger> .

:arg2 lemon:marker :school-5278_form_preposition .
## Prepositions ##
:school-5278_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

