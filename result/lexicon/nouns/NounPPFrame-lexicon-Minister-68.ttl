@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Minister-68 ;
  lemon:entry    :Minister-68_form_preposition .

:Minister-68 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Minister-68_form ;
  lemon:otherForm  :Minister-68_singular ;
  lemon:otherForm  :Minister-68_plural ;
  lemon:sense          :Minister-68_sense_1 ;
  lemon:synBehavior    :Minister-68_nounpp .

:Minister-68_form a lemon:Form ;
  lemon:writtenRep "Minister"@en .

:Minister-68_singular a    lemon:Form ;
  lemon:writtenRep "Minister"@en ;
  lexinfo:number   lexinfo:singular .

:Minister-68_plural a   lemon:Form ;
  lemon:writtenRep "Minister"@en ;
  lexinfo:number    lexinfo:plural .

:Minister-68_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Minister-68_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Minister-68_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/activeYearsEndDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Minister-68_sense_1_condition .

:Minister-68_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Athlete> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :Minister-68_form_preposition .
## Prepositions ##
:Minister-68_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

