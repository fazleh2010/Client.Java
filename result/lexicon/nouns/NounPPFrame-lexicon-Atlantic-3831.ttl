@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :Atlantic-3831 ;
  lemon:entry    :Atlantic-3831_form_preposition .

:Atlantic-3831 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :Atlantic-3831_form ;
  lemon:otherForm  :Atlantic-3831_singular ;
  lemon:otherForm  :Atlantic-3831_plural ;
  lemon:sense          :Atlantic-3831_sense_1 ;
  lemon:synBehavior    :Atlantic-3831_nounpp .

:Atlantic-3831_form a lemon:Form ;
  lemon:writtenRep "Atlantic"@en .

:Atlantic-3831_singular a    lemon:Form ;
  lemon:writtenRep "Atlantic"@en ;
  lexinfo:number   lexinfo:singular .

:Atlantic-3831_plural a   lemon:Form ;
  lemon:writtenRep "Atlantic"@en ;
  lexinfo:number    lexinfo:plural .

:Atlantic-3831_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:Atlantic-3831_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :Atlantic-3831_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/inflow> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :Atlantic-3831_sense_1_condition .

:Atlantic-3831_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/River> .

:arg2 lemon:marker :Atlantic-3831_form_preposition .
## Prepositions ##
:Atlantic-3831_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

