@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :opera-6211 ;
  lemon:entry    :opera-6211_form_preposition .

:opera-6211 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :opera-6211_form ;
  lemon:otherForm  :opera-6211_singular ;
  lemon:otherForm  :opera-6211_plural ;
  lemon:sense          :opera-6211_sense_1 ;
  lemon:synBehavior    :opera-6211_nounpp .

:opera-6211_form a lemon:Form ;
  lemon:writtenRep "opera"@en .

:opera-6211_singular a    lemon:Form ;
  lemon:writtenRep "opera"@en ;
  lexinfo:number   lexinfo:singular .

:opera-6211_plural a   lemon:Form ;
  lemon:writtenRep "opera"@en ;
  lexinfo:number    lexinfo:plural .

:opera-6211_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:opera-6211_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :opera-6211_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/producer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :opera-6211_sense_1_condition .

:opera-6211_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Company> .

:arg2 lemon:marker :opera-6211_form_preposition .
## Prepositions ##
:opera-6211_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

