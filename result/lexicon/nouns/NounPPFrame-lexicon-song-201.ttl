@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :song-201 ;
  lemon:entry    :song-201_form_preposition .

:song-201 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :song-201_form ;
  lemon:otherForm  :song-201_singular ;
  lemon:otherForm  :song-201_plural ;
  lemon:sense          :song-201_sense_1 ;
  lemon:synBehavior    :song-201_nounpp .

:song-201_form a lemon:Form ;
  lemon:writtenRep "song"@en .

:song-201_singular a    lemon:Form ;
  lemon:writtenRep "song"@en ;
  lexinfo:number   lexinfo:singular .

:song-201_plural a   lemon:Form ;
  lemon:writtenRep "song"@en ;
  lexinfo:number    lexinfo:plural .

:song-201_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:song-201_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :song-201_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/album> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :song-201_sense_1_condition .

:song-201_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Song> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Album> .

:arg2 lemon:marker :song-201_form_preposition .
## Prepositions ##
:song-201_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

