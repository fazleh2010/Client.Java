@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :station-1281 ;
  lemon:entry    :station-1281_form_preposition .

:station-1281 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :station-1281_form ;
  lemon:otherForm  :station-1281_singular ;
  lemon:otherForm  :station-1281_plural ;
  lemon:sense          :station-1281_sense_1 ;
  lemon:synBehavior    :station-1281_nounpp .

:station-1281_form a lemon:Form ;
  lemon:writtenRep "station"@en .

:station-1281_singular a    lemon:Form ;
  lemon:writtenRep "station"@en ;
  lexinfo:number   lexinfo:singular .

:station-1281_plural a   lemon:Form ;
  lemon:writtenRep "station"@en ;
  lexinfo:number    lexinfo:plural .

:station-1281_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:station-1281_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :station-1281_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/city> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :station-1281_sense_1_condition .

:station-1281_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :station-1281_form_preposition .
## Prepositions ##
:station-1281_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

