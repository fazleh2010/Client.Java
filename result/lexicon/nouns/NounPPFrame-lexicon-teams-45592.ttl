@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :teams-45592 ;
  lemon:entry    :teams-45592_form_preposition .

:teams-45592 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :teams-45592_form ;
  lemon:otherForm  :teams-45592_singular ;
  lemon:otherForm  :teams-45592_plural ;
  lemon:sense          :teams-45592_sense_1 ;
  lemon:synBehavior    :teams-45592_nounpp .

:teams-45592_form a lemon:Form ;
  lemon:writtenRep "teams"@en .

:teams-45592_singular a    lemon:Form ;
  lemon:writtenRep "teams"@en ;
  lexinfo:number   lexinfo:singular .

:teams-45592_plural a   lemon:Form ;
  lemon:writtenRep "teams"@en ;
  lexinfo:number    lexinfo:plural .

:teams-45592_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:teams-45592_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :teams-45592_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/nickname> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :teams-45592_sense_1_condition .

:teams-45592_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#String> .

:arg2 lemon:marker :teams-45592_form_preposition .
## Prepositions ##
:teams-45592_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

