@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :race-4428 ;
  lemon:entry    :race-4428_form_preposition .

:race-4428 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :race-4428_form ;
  lemon:otherForm  :race-4428_singular ;
  lemon:otherForm  :race-4428_plural ;
  lemon:sense          :race-4428_sense_1 ;
  lemon:synBehavior    :race-4428_nounpp .

:race-4428_form a lemon:Form ;
  lemon:writtenRep "race"@en .

:race-4428_singular a    lemon:Form ;
  lemon:writtenRep "race"@en ;
  lexinfo:number   lexinfo:singular .

:race-4428_plural a   lemon:Form ;
  lemon:writtenRep "race"@en ;
  lexinfo:number    lexinfo:plural .

:race-4428_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:race-4428_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :race-4428_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/location> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :race-4428_sense_1_condition .

:race-4428_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Building> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :race-4428_form_preposition .
## Prepositions ##
:race-4428_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

