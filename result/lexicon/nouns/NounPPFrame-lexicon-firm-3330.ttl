@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :firm-3330 ;
  lemon:entry    :firm-3330_form_preposition .

:firm-3330 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :firm-3330_form ;
  lemon:otherForm  :firm-3330_singular ;
  lemon:otherForm  :firm-3330_plural ;
  lemon:sense          :firm-3330_sense_1 ;
  lemon:synBehavior    :firm-3330_nounpp .

:firm-3330_form a lemon:Form ;
  lemon:writtenRep "firm"@en .

:firm-3330_singular a    lemon:Form ;
  lemon:writtenRep "firm"@en ;
  lexinfo:number   lexinfo:singular .

:firm-3330_plural a   lemon:Form ;
  lemon:writtenRep "firm"@en ;
  lexinfo:number    lexinfo:plural .

:firm-3330_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:firm-3330_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :firm-3330_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/headquarter> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :firm-3330_sense_1_condition .

:firm-3330_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/PopulatedPlace> .

:arg2 lemon:marker :firm-3330_form_preposition .
## Prepositions ##
:firm-3330_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

