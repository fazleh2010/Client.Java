@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :steam-2018 ;
  lemon:entry    :steam-2018_form_preposition .

:steam-2018 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :steam-2018_form ;
  lemon:otherForm  :steam-2018_singular ;
  lemon:otherForm  :steam-2018_plural ;
  lemon:sense          :steam-2018_sense_1 ;
  lemon:synBehavior    :steam-2018_nounpp .

:steam-2018_form a lemon:Form ;
  lemon:writtenRep "steam"@en .

:steam-2018_singular a    lemon:Form ;
  lemon:writtenRep "steam"@en ;
  lexinfo:number   lexinfo:singular .

:steam-2018_plural a   lemon:Form ;
  lemon:writtenRep "steam"@en ;
  lexinfo:number    lexinfo:plural .

:steam-2018_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:steam-2018_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :steam-2018_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/designer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :steam-2018_sense_1_condition .

:steam-2018_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Building> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :steam-2018_form_preposition .
## Prepositions ##
:steam-2018_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

